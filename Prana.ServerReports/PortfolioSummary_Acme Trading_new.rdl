        <CommandText>declare @MTDFromdate datetime,@QTDFromdate datetime,@YTDFromdate datetime, @MTDFromdateO datetime,@QTDFromdateO datetime,@YTDFromdateO datetime,@DailyO datetime
select @MTDFromdate =  CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(@EndDate)-1),@EndDate),101)
select @QTDFromdate =  DATEADD(qq, DATEDIFF(qq,0,@EndDate), 0)
select @YTDFromdate =  DATEADD(yy, DATEDIFF(yy,0,@EndDate), 0)


declare @t table
(
 Funds varchar(max),
 Daily float,
 Monthly float,
 Quarterly float,
 Yearly float,
 ITD datetime,
 CompanyFundID int,
 ID int identity(1,1)
)

insert into @t(Funds) values(@Fund)
insert into @t(Funds)
select * from dbo.Split(@Fund, ',')

update @t set CompanyFundID=(select CompanyFundID from T_CompanyFunds where FundName=Funds)
update @t set ITD= (select min(date) from pm_companyfundcashcurrencyvalue where FundID=CompanyFundID)
update @t set ITD= (select max(ITD) from @t) where ID='1'

update @t set Daily=100* dbo.F_getLinkedMDReturn(@EndDate,@EndDate, 'Fund', Funds)
 
update @t set Monthly=
CASE
WHEN @MTDFromdate&gt; =ITD Then
100* dbo.F_getLinkedMDReturn(@MTDFromdate,@EndDate, 'Fund', Funds)
ELSE Null
End
 
update @t set Quarterly=
CASE
WHEN @QTDFromdate&gt;=ITD Then
100*dbo.F_getLinkedMDReturn(@QTDFromdate,@EndDate, 'Fund', Funds) 
ELSE Null
End

update @t set Yearly=
CASE
WHEN @YTDFromdate&gt;=ITD Then
100*dbo.F_getLinkedMDReturn(@YTDFromdate,@EndDate, 'Fund', Funds) 
ELSE Null
End

update @t set Funds='' where ID='1'
select Funds,Daily,Monthly,Quarterly,Yearly from @t order by Funds</CommandText>
