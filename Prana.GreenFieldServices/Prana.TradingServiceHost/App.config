            <exceptionHandlers>
              <add type="Prana.LogManager.EnterpriseLibrary.MessageReportingExceptionHandler, Prana.LogManager.EnterpriseLibrary" name="Custom Handler" />
            </exceptionHandlers>
          </add>
        </exceptionTypes>
      </add>
      <add name="LogOnlyPolicy">
        <exceptionTypes>
          <add type="System.Exception, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" postHandlingAction="None" name="Exception">
            <exceptionHandlers>
