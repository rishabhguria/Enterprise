    <!--Regular expression for detecting asset classes based on symbol format, it is used in parsing ticker to MarketData symbols (i.e FactSet, ACTIV etc) -->
    <!--
      Format                       Pattern 
      MSFT                         ^[A-Z0-9]{1,10}$
      ABC.U                        ^[A-Z0-9]{1,10}.[A-Z]{1,10}$
      DL-EEB                       ^[A-Z0-9]{1,10}-[A-Z]{1,10}$
      RCOM.EQ-NSE                  ^[A-Z0-9]{1,10}.[A-Z]{1,10}-[A-Z]{1,10}$-->
    <add key="RegexOfEquityAsset" value="(^[A-Z0-9]{1,10}$)|(^[A-Z0-9]{1,10}.[A-Z]{1,10}$)|(^[A-Z0-9]{1,10}-[A-Z]{1,10}$)|(^[A-Z0-9]{1,10}.[A-Z]{1,10}-[A-Z]{1,10}$)" />
    <!--
      Format                       Pattern 
      O:AAPL 20A123.00             ^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}$
      O:AAPL 20A123.00D31          ^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}[D]([1-9]|1[0-9]|2[0-9]|3[0-1])$
      O:ATH 15A5.00-MXO            ^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}-[A-Z]{1,10}$
      O:ATH 15A5.00D30-MXO         ^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}[D]([1-9]|1[0-9]|2[0-9]|3[0-1])-[A-Z]{1,10}$-->
    <add key="RegexOfEquityOptionAsset" value="(^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}$)|(^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}[D]([1-9]|1[0-9]|2[0-9]|3[0-1])$)|(^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}-[A-Z]{1,10}$)|(^[O]{1}:[A-Z0-9]{1,10}\s[0-9]{2}[A-X]{1}[0-9]{1,10}.[0-9]{1,10}[D]([1-9]|1[0-9]|2[0-9]|3[0-1])-[A-Z]{1,10}$)" />
    <!--
      Format                       Pattern 
      NQ Z19                       ^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}$
      AX Z19-DT                    ^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}-[A-Z]{1,10}$-->
    <add key="RegexOfFutureAsset" value="(^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}$)|(^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}-[A-Z]{1,10}$)" />
    <!--
      Format                       Pattern 
      EW1 J23P1550                  ^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}[CP]{1}[0-9]{1,10}.[0-9]{1,10}$
      EW1 J23P1550D2                ^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}[CP]{1}[0-9]{1,10}.[0-9]{1,10}[D]([1-9]|1[0-9]|2[0-9]|3[0-1])$-->
    <add key="RegexOfFutureOptionAsset" value="(^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}[CP]{1}[0-9]{1,10}.[0-9]{1,10}$)|(^[A-Z0-9]{1,10}\s[FGHJKMNQUVXZ]{1}[0-9]{2}[CP]{1}[0-9]{1,10}.[0-9]{1,10}[D]([1-9]|1[0-9]|2[0-9]|3[0-1])$)" />
    <!--
      Format                       Pattern 
      AUD/USD                      ^[A-Z]{3}/[A-Z]{3}$-->
    <add key="RegexOfFXAsset" value="(^[A-Z]{3}/[A-Z]{3}$)" />
    <!--
      Format                       Pattern 
      USD/JPY 01/01/2024           ^[A-Z]{3}/[A-Z]{3} \d{2}/\d{2}/\d{4}$-->
    <add key="RegexOfFXForwardAsset" value="(^[A-Z]{3}/[A-Z]{3} \d{2}/\d{2}/\d{4}$)" />
    <!--
      Format                                      Pattern 
	  IBM US EQUITY ,K192390 KS EQUITY            ^[A-Za-z0-9]+ [A-Za-z]+ (?:EQUITY|Equity)$-->
    <add key="BloombergEquityRegex" value="(^[A-Za-z0-9]+ [A-Za-z]+ (?:EQUITY|Equity)$)" />
    <!--
      Format                             Pattern 
      CAM US 05/22/10 C37.5 Equity       ^[A-Za-z0-9]+\s[A-Za-z0-9]+\s\d{2}/\d{2}/\d{2}\s[A-Za-z]\d+(\.\d+)?\s(?:EQUITY|Equity)$-->
    <add key="BloombergEquityOptionRegex" value="(^[A-Za-z0-9]+\s[A-Za-z0-9]+\s\d{2}/\d{2}/\d{2}\s[A-Za-z]\d+(\.\d+)?\s(?:EQUITY|Equity)$)" />
    <!--
      Format                                                Pattern 
      NQU24 INDEX, NQU24 CURNCY,NQU24 COMDTY                (?i)^[A-Za-z0-9]*\d+\s(?:INDEX|CURNCY|COMDTY)$ -->
    <add key="BloombergFutureRegex" value="((?i)^[A-Za-z0-9]*\d+\s(?:INDEX|CURNCY|COMDTY)$)" />
    <!--
      Format                                                 Pattern 
      EW1J23 03/02/23 P1550 INDEX/CURNCY/COMDTY 
	  EW1J23 03/02/23 PS15.50 INDEX/CURNCY/COMDTY            (?i)^[A-Z0-9]+\s\d{2}/\d{2}/\d{2}\s[A-Z][A-Z\d]*(?:\.\d+)?\s(INDEX|CURNCY|COMDTY)$-->
    <add key="BloombergFutureOptionRegex" value="((?i)^[A-Z0-9]+\s\d{2}/\d{2}/\d{2}\s[A-Z][A-Z\d]*(?:\.\d+)?\s(INDEX|CURNCY|COMDTY)$)" />
    <!--
      Format                       Pattern 
      USDCAD BGN CURNCY 
      CHFUSD REGN CURNCY                
	  USDCAD CURNCY                (?i)^[A-Z]{6}(?:\s[A-Z]+)?\sCURNCY$-->
    <add key="BloombergFXRegex" value="((?i)^[A-Z]{6}(?:\s[A-Z]+)?\sCURNCY$)" />
    <!--
      Format                               Pattern 
      GBP/USD N 09/24/2024 CURNCY          (?i)^[A-Z/]+ \S+ \d{2}/\d{2}/\d{2,4} CURNCY$-->
    <add key="BloombergFXForwardRegex" value="((?i)^[A-Z/]+ \S+ \d{2}/\d{2}/\d{2,4} CURNCY$)" />
    <!--
      Format                               Pattern 
      ARGENT 1 07/09/29 Corp               ^\b[A-Z]{2,}\s\d+(\.\d+)?\s\d{2}/\d{2}/\d{2,4}\s\w+\b$-->
    <add key="BloombergFixedIncomeRegex" value="^\b[A-Z]{2,}\s\d+(\.\d+)?\s\d{2}/\d{2}/\d{2,4}\s\w+\b$" />
    <!--End Regular expression section-->
