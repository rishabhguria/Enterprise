    <DataSet Name="DataSetTodaysDate">
      <Fields>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>Declare @companyID int
Select @companyID = CompanyID from T_Company

Declare @auecID int
Select @auecID = DefaultAuecID from T_Company where companyID = @companyID

Select
Case dbo.IsBusinessDay(GetDate(),@auecID)
	When 1
	Then Convert(datetime,dbo.GetFormattedDatePart(GetDate()))
	Else Convert(datetime,dbo.GetFormattedDatePart(dbo.AdjustBusinessDays(GetDate(),-1,@auecID)))
End as StartDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
