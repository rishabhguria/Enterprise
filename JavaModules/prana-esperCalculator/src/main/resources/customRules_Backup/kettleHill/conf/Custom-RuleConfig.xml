		   		<constantValue displayName="% Gross Exposure" name="Percentage_GrossExposure_KH3" type="double" value="10"/>
				<constantValue displayName="Countries to be monitored" name="Country_KH3" type="String" value="Canada, Japan, Australia, Germany, 
				United Kingdom, Netherlands, France, Italy, Norway, Sweden, Finland, Denmark, Austria, Spain, Israel, SouthKorea, Ireland"/>
			</constantList>			
		</rule> 
			
