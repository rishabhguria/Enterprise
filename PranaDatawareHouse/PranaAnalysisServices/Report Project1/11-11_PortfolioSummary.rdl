,
 iif([Measures].[Opening Trade Date Key]&gt;=ActualYearStartDate.Item(0).Properties("Key",TYPED),
([Measures].[ClosedQty]
*[Measures].[Opening Side Multiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
*([Measures].[Closing Price]-[Measures].[Opening Price]))
-[Measures].[Closing Commission]
-[Measures].[Opening Commission]
,
[Measures].[ClosedQty]
*[Measures].[Opening Side Multiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
* ([Measures].[Closing Price]-MarkPriceClientConversionDate)
-[Measures].[Closing Commission]
)
)
)

member Yearly as
(
iif(
[T Custom Date Types].[Date Type].&amp;[ConversionDate].Properties("Date Key",TYPED)
 &gt; YearStartDate.Item(0).Properties("Key",TYPED),

   (
   [T Custom Date Types].[Date Type].&amp;[ConversionDate],  
   [Measures].[YTD Pnl])+
ConversionDateRealisedPnl+ConversionDateUnRealisedPnl
 ,
YearlyRealisedPnl+YearlyUnRealisedPnl
 )
)
member CumPnl as
(
([T Custom Date Types].[Date Type].&amp;[ConversionDate],[Measures].[Inception To Date Pnl])
  +
ConversionDateRealisedPnl+ConversionDateUnRealisedPnl
 )

