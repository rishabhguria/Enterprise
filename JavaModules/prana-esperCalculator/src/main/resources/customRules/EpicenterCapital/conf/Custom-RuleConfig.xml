			<enabled>true</enabled>
			<isDeleted>false</isDeleted>
			<ruleType>PreTrade</ruleType>
			<eplPath>/preTradeCheckSideAccountSymbol/</eplPath>
			<description>Trade would sell more than the current position</description>
			<compressionLevel>Custom</compressionLevel>
			<validationCompletedEventName>PreTradeCheckSideAccountSymbol</validationCompletedEventName>
			<outputStatementList>
				<outputStatementName name="PreTradeCheckSideAccountSymbol"/>
			</outputStatementList>
			<windowFillerStatementList>
				<windowFillerStatementName name="PreTradeCheckSideAccountSymbol"/>
			</windowFillerStatementList>
			<blocked>true</blocked>
			<constantList> 
			</constantList>	
		</rule> 
