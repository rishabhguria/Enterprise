            <Value>=Join(Parameters!fund.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="OpeningEquity">
      <Fields>
        <Field Name="Daily">
          <DataField>Daily</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Monthly">
          <DataField>Monthly</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Quarterly">
          <DataField>Quarterly</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Yearly">
          <DataField>Yearly</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>declare @MTDFromdate datetime,@QTDFromdate datetime,@YTDFromdate datetime, @MTDFromdateO datetime,@QTDFromdateO datetime,@YTDFromdateO datetime,@DailyO datetime

select @MTDFromdate =  CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(@EndDate)-1),@EndDate),101)
select @QTDFromdate =  DATEADD(qq, DATEDIFF(qq,0,@EndDate), 0)
select @YTDFromdate =  DATEADD(yy, DATEDIFF(yy,0,@EndDate), 0)

Select @DailyO=dateadd(day,datediff(day,1,@EndDate),0)
Select @DailyO=
Case
When DATEPART(dw, @DailyO) in (1,7) Then
dbo.AdjustBusinessDays(@EndDate,-1,11)
Else
@DailyO
End

select @MTDFromdateO=dbo.AdjustBusinessDays(@MTDFromdate,-1,11)
select @QTDFromdateO=dbo.AdjustBusinessDays(@QTDFromdate,-1,11)
select @YTDFromdateO=dbo.AdjustBusinessDays(@YTDFromdate,-1,11)


select @DailyO= case When @DailyO&lt;=@ITD then @ITD Else @DailyO end
select @MTDFromdateO= case When @MTDFromdateO&lt;=@ITD then @ITD Else @MTDFromdateO end
select @QTDFromdateO= case When @QTDFromdateO&lt;=@ITD then @ITD Else @QTDFromdateO end
select @YTDFromdateO= case When @YTDFromdateO&lt;=@ITD then @ITD Else @YTDFromdateO end


declare @t table
(
 Daily float,
 Monthly float,
 Quarterly float,
 Yearly float
)

insert into @t
select 
dbo.F_D_getNAV_MW(@DailyO, 'fund', @fund),
dbo.F_D_getNAV_MW(@MTDFromdateO, 'fund', @fund),
dbo.F_D_getNAV_MW(@QTDFromdateO, 'fund', @fund),
dbo.F_D_getNAV_MW(@YTDFromdateO, 'fund', @fund)


