          <Value>=IIF(Fields!PositionFrom.Value = "O",
IIF(DateValue(Fields!TradeDate.Value.ToString()) &gt;= DateValue(Parameters!StartDate.Value.ToString()) ,
(Fields!MarketValue2.Value - Fields!MarketValue1.Value - Fields!TotalOpenCommission.Value),
(0.0)),0.0)</Value>
        </Field>
