    <add key="ServiceDescription" value="Prana Trading Green Field Service." />
    <add key="OrderRequestPort" value="5010" />
    <add key="AutoReconnectInterval" value="10" />
	<add key="TradeServer" value="0.0.0.0:5001" /> 
    <add key="KafkaConfigPath" value="D:\\ThirdPartySoftwares\\Kafka\\config\\config_clientname.xml" />
    <add key="DuplicateTradeCheckExpirationTime" value="60" />
    <!--*******SERILOG CONFIGURATIONS********-->
    <!-- Console Sink -->
    <add key="serilog:using:Console" value="Serilog.Sinks.Console" />
    <add key="serilog:write-to:Console.outputTemplate" value="{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj} [User:{UserId}, CorrelationId:{CorrelationId})]{NewLine}{Exception}{NewLine}" />
    <!-- **TO ADD SEQ, USE THIS**
		<add key="serilog:using:Seq" value="Serilog.Sinks.Seq" />
		<add key="serilog:write-to:Seq.serverUrl" value="http://localhost:5341" />
		 -->
    <!-- Rolling File -->
    <add key="serilog:using:File" value="Serilog.Sinks.File" /> 
    <add key="serilog:write-to:File.fileSizeLimitBytes" value="5242880" />
    <!--5 MB SIZE-->
    <add key="serilog:write-to:File.outputTemplate" value="{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj} [User:{UserId}, CorrelationId:{CorrelationId}, Source:{CustomSource})]{NewLine}{Exception}{NewLine}" />
    <!-- Minimum Level, Can be Verbose, Information, Warning-->
    <add key="serilog:minimum-level" value="Debug" />
    <add key="serilog:enrich:with-property:AppName" value="TradingService" />
    <!--*******END OF SERILOG CONFIGURATIONS********-->
    <add key="ConfigSectionNamesForLogAtApplicationStartUp" value="appSettings, connectionStrings" />
  </appSettings>
