With
set [FirstDate] as
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).Lag(10):strtomember(@ToDimTimeDate,                CONSTRAINED)
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)
set [PrevDate] as
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).PrevMember.Lag(10):
strtomember(@ToDimTimeDate,CONSTRAINED).PrevMember
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)
)
)
set[MonthMarkDate] as 
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).Parent.FirstChild.Prevmember.Lag(10):
strtomember(@ToDimTimeDate,CONSTRAINED).Parent.FirstChild.Prevmember
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)
)
)
set[QuarterMarkDate] as 
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).Parent.Parent.FirstChild.FirstChild.Prevmember.Lag(10):
strtomember(@ToDimTimeDate,CONSTRAINED).Parent.Parent.FirstChild.FirstChild.Prevmember
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)
)
)
set[YearMarkDate] as 
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).Parent.Parent.Parent.FirstChild.FirstChild.FirstChild.Prevmember.Lag(10):
strtomember(@ToDimTimeDate,CONSTRAINED).Parent.Parent.Parent.FirstChild.FirstChild.FirstChild.Prevmember
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)
)
)

member [DailyReturn] as
(
IIF((FirstDate.item(0),[Measures].[Last Value])&gt;0 and (PrevDate.item(0),[Measures].[Last Value])&gt;0,
((FirstDate.item(0),[Measures].[Last Value])-(PrevDate.item(0),[Measures].[Last Value]))
/(PrevDate.item(0),[Measures].[Last Value]),0)*100
)
member [MonthlyReturn] as
(
IIF((FirstDate.item(0),[Measures].[Last Value])&gt;0 and (MonthMarkDate.item(0),[Measures].[Last Value])&gt;0,
((FirstDate.item(0),[Measures].[Last Value])-(MonthMarkDate.item(0),[Measures].[Last Value]))/(MonthMarkDate.item(0),[Measures].[Last Value])*100
,0)
)
member [QuarterlyReturn] as 
(
IIF((FirstDate.item(0),[Measures].[Last Value])&gt;0 and (QuarterMarkDate.item(0),[Measures].[Last Value])&gt;0,
((FirstDate.item(0),[Measures].[Last Value])-(QuarterMarkDate.item(0),[Measures].[Last Value]))/(QuarterMarkDate.item(0),[Measures].[Last Value]),0)*100
)
member [YearlyReturn] as 
(
IIF((FirstDate.item(0),[Measures].[Last Value])&gt;0 and (YearMarkDate.item(0),[Measures].[Last Value])&gt;0,
((FirstDate.item(0),[Measures].[Last Value])-(YearMarkDate.item(0),[Measures].[Last Value]))/(YearMarkDate.item(0),[Measures].[Last Value]),0)*100
)



