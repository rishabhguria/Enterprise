set [FirstDate] as
(


Tail(
crossjoin
(
   {
      strtomember(@FirstDimTime).Lag(10):strtomember(@FirstDimTime)
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)

set [SecondDate] as
(
Tail(
crossjoin
(
   {
      strtomember(@SecondDimTime).Lag(10):strtomember(@SecondDimTime)
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)
member [SymbolLastQuantity] as
(
Sum(Tail(
Filter(
(null:[Dim Time].[Calender].CurrentMember)*
[PM Open Position Snap Shots].[Symbol].Currentmember,
not isempty([Measures].[SymbolQuantity])
),1),[Measures].[SymbolQuantity]
)
)


member [LongDeltaAdjExposure]
as
(
sum(
Filter(
[PM Open Position Snap Shots].[Symbol].children
*[PM Open Position Snap Shots].[Under Lying Symbol].children
,[SymbolLastQuantity]*([Dim Time].[Calender].CurrentMember,[Measures].[DeltaDefault]) &gt;0),

(LinkMember([PM Open Position Snap Shots].[Under Lying Symbol].CurrentMember,
 [PM Open Position Snap Shots].[Fund-Symbol]),
([Dim Time].[Calender].CurrentMember,[Measures].[Final Mark Price]))
*([SymbolLastQuantity],[Dim Time].[Calender].CurrentMember)
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
*([Dim Time].[Calender].CurrentMember,[Measures].[DeltaDefault])

)
)
member [ShortDeltaAdjExposure]
as
(
sum(
Filter(
[PM Open Position Snap Shots].[Symbol].children
*[PM Open Position Snap Shots].[Under Lying Symbol].children
,[SymbolLastQuantity]*([Dim Time].[Calender].CurrentMember,[Measures].[DeltaDefault]) &lt;0),

(LinkMember([PM Open Position Snap Shots].[Under Lying Symbol].CurrentMember,
 [PM Open Position Snap Shots].[Fund-Symbol]),
([Dim Time].[Calender].CurrentMember,[Measures].[Final Mark Price]))
*([SymbolLastQuantity],[Dim Time].[Calender].CurrentMember)
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
*([Dim Time].[Calender].CurrentMember,[Measures].[DeltaDefault])

)
)


member [FirstdateLongDelta] as 
(
([LongDeltaAdjExposure],FirstDate.item(0))
)

member [SeconddateLongDelta] as 
(
([LongDeltaAdjExposure],SecondDate.item(0))
)
member [AvgLongDelta]
as
(
Sum(
 
(FirstDate.Item(0):SecondDate.Item(0)),
LongDeltaAdjExposure
 )/count(FirstDate.Item(0):SecondDate.Item(0))
)

member [FirstdateShortDelta] as 
(
([ShortDeltaAdjExposure],FirstDate.item(0))
)

member [SeconddateShortDelta] as 
(
([ShortDeltaAdjExposure],SecondDate.item(0))
)
member [AvgShortDelta]
as
(
Sum(
 
(FirstDate.Item(0):SecondDate.Item(0)),
ShortDeltaAdjExposure
 )/count(FirstDate.Item(0):SecondDate.Item(0))
)




 
SELECT 
 { FirstdateLongDelta,SeconddateLongDelta,AvgLongDelta,
FirstdateShortDelta,SeconddateShortDelta,AvgShortDelta} ON COLUMNS,
{[Dim DW Account].[DW Account Name].[All]}
 ON ROWS from
(  select {  ([Dim DW Account].[Account Type].&amp;[1]),
([Dim DW Account].[Account Type].&amp;[2])  } on 
COLUMNS FROM ( SELECT ( STRTOSET(@TCompanyFundsFundName, CONSTRAINED) ) 
ON COLUMNS from [Nirvana Client DW]))



