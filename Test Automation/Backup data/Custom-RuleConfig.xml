			<name>Order Side Check Having No MarketPermission</name>
			<enabled>false</enabled>
			<isDeleted>false</isDeleted>
			<ruleType>PreTrade</ruleType>
			<eplPath>/preTradeCheckSideAccountSymbolPost/</eplPath>
			<description>Pre-Trade Alerts For OrderSide Violation on Account-Symbol Level</description>
			<compressionLevel>Custom~ Account~Symbol</compressionLevel>
			<validationCompletedEventName>PreTradeCheckSideAccountSymbolPost</validationCompletedEventName>
			<outputStatementList>
				<outputStatementName name="PreTradeCheckSideAccountSymbolPost"/>
			</outputStatementList>
			<windowFillerStatementList>
				<windowFillerStatementName name="PreTradeCheckSideAccountSymbolPost"/>
			</windowFillerStatementList>
			<blocked>true</blocked>
			<constantList> 
			</constantList>	
		</rule>	 -->
		 <rule id="custom-rule-MR8">
               <name>5% PreTradeSharesOutstandingRangeAlert</name>
               <enabled>true</enabled>
               <isDeleted>false</isDeleted>
               <ruleType>PreTrade</ruleType>
               <eplPath>/PreTradeSharesOutstandingRangeAlert/</eplPath>
               <description>A security in the portfolio has a quantity greater than 5% of the shares outstanding.</description>
               <compressionLevel>Custom</compressionLevel>
               <validationCompletedEventName>PreTradeSharesOutstandingRangeAlert</validationCompletedEventName>
               <outputStatementList>
                    <outputStatementName name="PreTradeSharesOutstandingRangeAlert"/>
               </outputStatementList>
               <windowFillerStatementList>
                    <windowFillerStatementName name="PreTradeSharesOutstandingRangeAlert"/>
               </windowFillerStatementList>
               <constantList>
			   <constantValue displayName="Shares Outstanding Lower Limit %" name="SharesOutstandingLowerLimitPercentage" type="double" value="5"/>
				<constantValue displayName="Shares Outstanding Upper Limit %" name="SharesOutstandingUpperLimitPercentage" type="double" value="10"/>
				<constantValue displayName="MasterFund Name" name="MasterFundNameRangeAlert" type="string" value="Maple Rock Master Fund LP"/>
				<constantValue displayName="Summary" name="SummaryDescriptionRangeAlert" type="string" value="Shares outstanding 5 % has been breached"/>
			   </constantList>
               <blocked>true</blocked>
          </rule>	
	    	 <rule id="custom-rule-MR9">
               <name>10% PreTradeSharesOutstandingBreachAlert</name>
               <enabled>true</enabled>
               <isDeleted>false</isDeleted>
               <ruleType>PreTrade</ruleType>
               <eplPath>/PreTradeSharesOutstandingBreachAlert/</eplPath>
               <description>A security in the portfolio has a quantity greater than 10% of the shares outstanding.</description>
               <compressionLevel>Custom</compressionLevel>
               <validationCompletedEventName>PreTradeSharesOutstandingBreachAlert</validationCompletedEventName>
               <outputStatementList>
                    <outputStatementName name="PreTradeSharesOutstandingBreachAlert"/>
               </outputStatementList>
               <windowFillerStatementList>
                    <windowFillerStatementName name="PreTradeSharesOutstandingBreachAlert"/>
               </windowFillerStatementList>
               <constantList>
			   <constantValue displayName="Shares Outstanding Breach %" name="SharesOutstandingBreachPercentage" type="double" value="10"/>
				<constantValue displayName="MasterFund Name" name="MasterFundNameConstant" type="string" value="Maple Rock Master Fund LP"/>
				<constantValue displayName="Summary" name="SummaryDescription" type="string" value="Shares outstanding 10 % has been breached"/>
			   </constantList>
               <blocked>true</blocked>
          </rule>
	</rules>
</ruleConfig>

