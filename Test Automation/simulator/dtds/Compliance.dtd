<!ELEMENT ComplianceFilterSets (ComplianceFilterSet*)>
<!ATTLIST ComplianceFilterSets
          class     CDATA #FIXED "com.cameronsystems.fix.compliance.ComplianceFilterSetDictionary"
>

<!ELEMENT ComplianceFilterSet (ConcurrentOrderFilter | InadvertentCrossingFilter | GrossExposureFilter | MarketModeFilter | NetExposureFilter | OrderValueFilter | PercentageFromAuctionPriceFilter | PercentageFromBestPriceFilter | PercentageFromLastPriceFilter | PercentageFromMarketPriceFilter | PriceStepsFromAuctionPriceFilter | PriceStepsFromBestPriceFilter | PriceStepsFromLastPriceFilter | PriceStepsFromMarketPriceFilter | SecurityFilter | SecurityTypeFilter )* >
<!ATTLIST ComplianceFilterSet
          class     CDATA #FIXED "com.cameronsystems.fix.compliance.ComplianceFilterSet"
          id         CDATA #REQUIRED
          refid      CDATA #IMPLIED
>

<!ELEMENT ConcurrentOrderFilter EMPTY>
<!ATTLIST ConcurrentOrderFilter
          class     CDATA #FIXED "com.cameronsystems.fix.compliance.ConcurrentOrderFilter"
          enabled   ( true | false ) "true"
          autoReject  ( true | false ) "true"
          tolerance CDATA #REQUIRED
          clientEntityMapper  CDATA "com.cameronsystems.fix.compliance.DefaultClientEntityMapper"
>

<!ELEMENT InadvertentCrossingFilter EMPTY>
<!ATTLIST InadvertentCrossingFilter
          class     CDATA #FIXED "com.cameronsystems.fix.compliance.InadvertentCrossingFilter"
          enabled   ( true | false ) "true"
          autoReject  ( true | false ) "true"
          clientEntityMapper  CDATA "com.cameronsystems.fix.compliance.DefaultClientEntityMapper"
>

<!ELEMENT GrossExposureFilter EMPTY>
<!ATTLIST GrossExposureFilter
          class     CDATA #FIXED "com.cameronsystems.fix.compliance.GrossExposureFilter"
          enabled   ( true | false ) "true"
          autoReject  ( true | false ) "true"
          tolerance CDATA #REQUIRED
          clientEntityMapper  CDATA "com.cameronsystems.fix.compliance.DefaultClientEntityMapper"
>

<!ELEMENT MarketModeFilter EMPTY>
<!-- Allowed modes are a comma separated list of market modes.
     For SEATS, this might PL,PE for production and TL,TE for test.
 -->
<!ATTLIST MarketModeFilter
          class     CDATA #FIXED "com.cameronsystems.fix.compliance.MarketModeFilter"
          enabled   ( true | false ) "true"
          autoReject  ( true | false ) "true"
          allowed   CDATA #REQUIRED
>

