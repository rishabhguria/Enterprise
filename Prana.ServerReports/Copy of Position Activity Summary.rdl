		IIF(Fields!PositionFrom.Value.Equals("C") And DateValue(Fields!TradeDate.Value.ToString()) &lt;= DateValue(Parameters!StartDate.Value.ToString()) And DateValue(Parameters!StartDate.Value.ToString()) &lt;= DateValue(Fields!ClosingDate.Value.ToString()),
		Fields!TaxLotOpenQty.Value,
			 IIF(DateValue(Fields!TradeDate.Value.ToString()) &gt;= DateValue(Parameters!StartDate.Value.ToString()) And Fields!PositionFrom.Value.Equals("I"),
			 Fields!TaxLotOpenQty.Value,0.0)
		)
 )</Value>
        </Field>
        <Field Name="PNLTrades">
          <Value>=Fields!OpenPNLTrades.Value + Fields!RealizedPNL.Value</Value>
        </Field>
