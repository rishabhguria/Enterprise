<?xml version="1.0" encoding="ISO-8859-1"?>
<appConfig>
  <configSections>
    <!-- This section contains the settings of configuration file. It is mandatory 
			to include a section tag if any new section is included -->
    <section key="AppSettings" value="appSettings.appSetting" />
    <section key="CustomRuleConfigPaths" value="customRuleConfigPaths.customRuleConfigPath" />
    <section key="Exchanges" value="amqpMedia.exchanges.exchange" />
    <section key="Queues" value="amqpMedia.queues.queue" />
    <section key="StatementsToExchange" value="outStatements.toExchange.statement" />
    <section key="StatementsToQueue" value="outStatements.toQueue.statement" />
    <section key="NamedWindowList" value="namedWindows.window" />
    <section key="StatementsToLog" value="loggingStatements.statement" />
    <section key="DivisorMemoryMonitor" value="divisorMemoryMonitor.setting" />
    <section key="StatementsForDetection" value="detectionStatements.statement" />
  </configSections>
  <divisorMemoryMonitor>
    <setting key="MemoryCheckInterval" value="10" />
    <setting key="UsedMemoryPercentage" value="85" />
    <setting key="TimerIncrement" value="2" />
    <setting key="PercentageIncrement" value="0.5" />
    <setting key="MaxIncrementLimit" value="5" />
    <setting key="TimeToPercentageRatio" value="3" />
  </divisorMemoryMonitor>
  <appSettings>
    <!-- Basic application settings -->
    <appSetting key="AmqpServer" value="localhost" />
    <appSetting key="Vhost" value="Dev" />
    <appSetting key="UserId" value="nirvana" />
    <appSetting key="Password" value="PRana888" />
    <appSetting key="StartStatementMonitor" value="true" />
    <!-- To enable logging permission is required in config of both esperCalculator 
			and loggingTool -->
    <appSetting key="EventsLogging" value="false" />
    <appSetting key="EsperConfigFile" value="target/resources/conf/EsperEngine.config.xml" />
    <appSetting key="MonitoringEpl" value="target/resources/monitoringEpl/" />
    <appSetting key="DetectionEpl" value="target/resources/detectionEPL" />
    <appSetting key="EplDirectory" value="target/resources/eplModules/" />
    <appSetting key="InterceptorDirectory" value="target/resources/interceptor/" />
    <appSetting key="WindowValidatorDirectory" value="target/resources/windowValidator" />
    <appSetting key="TTLForLiveFeedQueue" value="10000" />
    <!-- If SymbolDataUpdateLogging is true, then Symbol Update messages of each symbol will be logged  -->
    <appSetting key="SymbolDataUpdateLogging" value="false" />
    <!-- A port lock will be taken on this so that multiple instances should 
			not run -->
    <appSetting key="SocketLockPort" value="5041" />
    <appSetting key="WindowDumpDirectory" value="dump" />
    <appSetting key="WindowDetectionDumpDirectory" value="detection_dumps" />
    <!-- list of assets whose prices will be received from live feed, use '/' 
			to separate the assets -->
    <appSetting key="PricingRequired" value="Equity/EquityOption/Future/FutureOption/FX/Indices/FixedIncome/PrivateEquity/FXOption/FXForward/Forex/ConvertibleBond/CreditDefaultSwap" />
    <appSetting key="SymbolsRequireLogging" value="" />
    <appSetting key="DumpIntervalForDetection" value="300" />
    <appSetting key="TaxlotHelperDelay" value="1" />
    <appSetting key="SymbolDataHelperDelay" value="2" />
    <appSetting key="SleepPostOnWhatIf" value="3" />
    <!-- If IsLiveMode config value is true, then calculation will be done 
			on actual live prices If IsLiveMode config value is false, then calculation 
			will be done on manual prices (According to +2 and -2 logic) -->
    <appSetting key="IsLiveMode" value="true" />
    <appSetting key="RELEASE_FOLDER_NAME_ESPER" value="EsperCalculator" />
    <appSetting key="RELEASE_FOLDER_NAME_BASKET" value="BasketComplianceService" />
    <appSetting key="EsperCalculation" value="prana-esperCalculator" />
    <appSetting key="BasketCompliance" value="prana-basketComplianceService" />
    <appSetting key="SecurityRetryCount" value="10" />
    <!-- Email settings -->
    <appSetting key="HostName" value="smtp.nirvanasolutions.com" />
    <appSetting key="SMTPPort" value="2525" />
    <appSetting key="MailUserName" value="alerts1@nirvanasolutions.com" />
    <appSetting key="MailPassword" value="nirvana888" />
    <appSetting key="SenderName" value="Compliance Helper" />
    <!-- Use '/' character for multiple email ids to define in RecieverTo, 
			RecieverCC and RecieverBCC -->
    <appSetting key="RecieverTo" value="shagoon.gurtata@nirvanasolutions.com" />
    <appSetting key="RecieverCC" value="shagoon.gurtata@nirvanasolutions.com" />
    <appSetting key="RecieverBCC" value="shagoon.gurtata@nirvanasolutions.com" />
    <!-- This should be only a single email id -->
    <appSetting key="BounceAddress" value="rajat.tandon@nirvanasolutions.com" />
    <!-- Email settings -->
    <!-- HeartBeatTimer value is in milliseconds -->
    <appSetting key="HeartBeatTimer" value="5000" />
    <appSetting key="PricingTimeout" value="30000" />
    <appSetting key="MissingInformationRequired" value="false" />
    <!-- Keep IsSymbolDataEOMRequiredOnStalePrices true if User have compliance permission. -->
    <!-- Set IsSymbolDataEOMRequiredOnStalePrices false only if-
    	1. User doesn't have compliance permission.
    	2. Post trade compliance permission is disabled.
		3. Post trade compliance permission is enabled but all post trade rule is disabled.-->
    <appSetting key="IsSymbolDataEOMRequiredOnStalePrices" value="true" />
    <!-- Set true if EPL compilation is required else false -->
    <appSetting key="IsEPLCompilationRequired" value="false" />
    <!-- Set period of time after which to compiler EPL's again -->
    <appSetting key="NoOfDaysToCompileEPLAfter" value="1" />
    <!--Set precision after decimal for Order Side Rule Quantity -->
    <appSetting key="PrecisionAfterDecimal" value="10" />
    <!-- This setting is to enable or disable email notification of error occurred 
			in prana-esperCalculator project -->
    <appSetting key="EmailErrorNotificationPermission" value="false" />
    <!-- This setting is to set the time window(in seconds) within which same 
			errors should not be notified on email more than once -->
    <appSetting key="EmailErrorNotificationTimeoutSeconds" value="300" />
    <appSetting key="InterceptorFileName" value="symbol.csv" />
  </appSettings>
