        <CommandText>DECLARE @MTDFromdate datetime,@QTDFromdate datetime,@YTDFromdate datetime, @MTDFromdateO datetime,@QTDFromdateO datetime,@YTDFromdateO datetime


SELECT @MTDFromdate =  CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(@EndDate)-1),@EndDate),101)
SELECT @QTDFromdate =  DATEADD(qq, DATEDIFF(qq,0,@EndDate), 0)
SELECT @YTDFromdate =  DATEADD(yy, DATEDIFF(yy,0,@EndDate), 0)

SELECT @MTDFromdate= case When @MTDFromdate&lt;=@ITD then @ITD Else @MTDFromdate end
SELECT @QTDFromdate= case When @QTDFromdate&lt;=@ITD then @ITD Else @QTDFromdate end
SELECT @YTDFromdate= case When @YTDFromdate&lt;=@ITD then @ITD Else @YTDFromdate end

--print @QTDFromdate

DECLARE @j table
(
 SubCategoryID int,
 SubCategoryName varchar(max),
 Name varchar(max),
 TransactionTypeID int,
 SeqNo int,
 FundID int,
 SubAccountID int,
 CurrencyID int,
 TransactionDate datetime,
 CurrentDR float,
 CurrentCR float,
 Count int,
MasterCategoryID int
)

--Need to add master category as well in the below table
insert into @j
SELECT 
a. subcategoryID, 
a.SubCategoryName ,
b.Name,
b.TransactionTypeID,
c.SeqNo,
c.FundID,
c.SubAccountID,
c.CurrencyID,
c.TransactionDate,
c.CurrentDR,
c.CurrentCR,
c.Count,
MC.MasterCategoryID
from T_SubCategory a
inner join T_MasterCategory MC on a.MasterCategoryID = MC.MasterCategoryID
inner join T_SubAccounts b on b.acronym&lt;&gt;'cash' and a.SubCategoryID=b.SubCategoryID
inner join V_SubAccountDayBalance c on b.SubaccountID=c.SubaccountID 
and FundID in 
	(SELECT CompanyFundID from t_companyfunds 
	WHERE FundName in( SELECT * from dbo.split(@Fund,',')))
order by transactionDate

--Ashish 20121217: We can remove all TRA type transactions like Commission, FX Gain etc. As these are included already in the Realized Gain/Loss Section
delete  from @j WHERE TransactionTypeID =
                          (SELECT TransactionTypeID FROM T_TransactionType
                            WHERE TransactionTypeAcronym = 'TRA') 
--and SubaccountID not in (39,40) ----Ashish 20121217: Remove FX Gain/Loss accounts for now as FX settlement can be done on Closing Price

-- Ashish 20121217: Remove all accounts except Income and expenses from the journal entry list
delete from @j WHERE MasterCategoryID not in('3','4','5') 
--and SubaccountID not in (39,40) 
--SELECT * from @j
-------------------------------FX Convension-------------------------------------------------
DECLARE  @FXConversionRates Table                       
(                                                                                                                                                  
 FromCurrencyID int,         
 ToCurrencyID int,                                               
 RateValue float,                 
 ConversionMethod int,                                                                         
 Date DateTime,                        
 eSignalSymbol varchar(max)
                                                 
)  

Insert into @FXConversionRates        
Exec P_GetAllFXConversionRatesForGivenDateRange @YTDFromdate,@EndDate                                                                                                                                 
--  Select * from  dbo.GetAllFXConversionRatesForGivenDateRange(@MinTradeDate,@EndDate) as A                                                                                                                               
                                                        
 UPDATE @FXConversionRates                                                                           
 Set RateValue = 1.0/RateValue                                                                                                                                
 Where RateValue &lt;&gt; 0 and ConversionMethod = 1                                                                                                                                
                                                                                     
UPDATE @FXConversionRates                                                                                                                             
 Set RateValue = 0                                                        
 Where RateValue is Null      

--SELECT * from @FXConversionRates WHERE conversionMethod ='1'order by date
------------------------------------------------------------------------------------------------------------
UPDATE @j 
SET CurrentDR = CurrentDR * RateValue, 
CurrentCR = CurrentCR * RateValue 
from @FXConversionRates 
WHERE ToCurrencyID = '1' and Date=TransactionDate and CurrencyID = FromCurrencyID

UPDATE @j SET CurrentDR=-CurrentDR 


DECLARE @EquityItems table
(
 Items varchar(max),
 Daily float,
 Monthly float,
 Quarterly float,
 Yearly float
)
INSERT into @EquityItems(Items) 
SELECT subCategoryName  from T_SubCategory Sub
inner join T_MasterCategory MC on Sub.MasterCategoryID = MC.MasterCategoryID
--Removing Asset and Liabilities.
WHERE Sub.SubCategoryName &lt;&gt; 'Cash' 
and MC.MasterCategoryID not in (1,2) 

Insert into @EquityItems (Items)  values ('Accruals')
Insert into @EquityItems (Items)  values ('Interest Income/Expense')
Insert into @EquityItems (Items)  values ('Gross Dividend Income/Expense')
Insert into @EquityItems (Items)  values ('Misc Income/Expense')
Insert into @EquityItems (Items)  values ('Realized Unrealized Gain/(Loss) on Securities')
Insert into @EquityItems (Items)  values ('Unrealized Gain On Cash')


------------------------------------------------Daily----------------------------------------------------------
UPDATE @EquityItems 
SET Daily =(SELECT sum( CurrentDR+CurrentCR) from @j 
             WHERE TransactionDate=@EndDate
             and SubCategoryName=Items)

UPDATE @EquityItems 
SET Daily = isnull((SELECT sum(TotalRealizedPNLOnCost)+Sum(ChangeInUnrealizedPNL) from T_MW_genericPnL
                  WHERE rundate=@EndDate and asset &lt;&gt; 'cash'
                  and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
               WHERE Items = 'Realized Unrealized Gain/(Loss) on Securities'

UPDATE @EquityItems 
SET Daily = isnull((SELECT sum(UnrealizedTotalGainOnCostD2_Base) from T_MW_genericPnL
                  WHERE rundate=@EndDate and asset = 'cash'
                  and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
                   WHERE Items = 'Unrealized Gain On Cash'

UPDATE @EquityItems 
SET Daily = (SELECT sum(CurrentDR + CurrentCR) 
			 from @j WHERE TransactionDate = @EndDate and 
            (SubCategoryName='Interest Expense' or SubCategoryName='Interest Income'))
			 WHERE Items = 'Interest Income/Expense'

UPDATE @EquityItems 
SET Daily = (SELECT sum(CurrentDR + CurrentCR) 
			from @j WHERE TransactionDate = @EndDate and 
            (SubCategoryName='Misc Expense' or SubCategoryName='Misc Income'))
		     WHERE Items = 'Misc Income/Expense'

UPDATE @EquityItems 
SET Daily = (SELECT sum(Dividend) 
			 from T_MW_genericPnL WHERE rundate = @EndDate and Open_CloseTag = 'D'
             and Fund in  (SELECT * from dbo.Split(@Fund, ',')))
		     WHERE Items = 'Gross Dividend Income/Expense'

-------------------------------------------------Monthly-------------------------------------------------------

UPDATE @EquityItems 
SET Monthly = (SELECT sum( CurrentDR+CurrentCR) from @j 
             WHERE TransactionDate between @MTDFromdate and @EndDate
             and SubCategoryName=Items)

UPDATE @EquityItems 
SET Monthly = isnull((SELECT sum(TotalRealizedPNLOnCost)+Sum(ChangeInUnrealizedPNL) from T_MW_genericPnL
              WHERE rundate  between @MTDFromdate and @EndDate and asset &lt;&gt; 'cash'
              and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
              WHERE Items='Realized Unrealized Gain/(Loss) on Securities'

UPDATE @EquityItems 
SET Monthly = isnull((SELECT sum(UnrealizedTotalGainOnCostD2_Base) from T_MW_genericPnL
              WHERE rundate  between @MTDFromdate and @EndDate and asset = 'cash'
              and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
              WHERE Items='Unrealized Gain On Cash'


UPDATE @EquityItems 
SET Monthly = (SELECT sum(CurrentDR + CurrentCR) 
			   from @j WHERE (TransactionDate between @MTDFromdate and @EndDate) and 
			  (SubCategoryName='Interest Expense' or SubCategoryName='Interest Income'))
			   WHERE Items='Interest Income/Expense'

UPDATE @EquityItems 
SET Monthly = (SELECT sum(CurrentDR + CurrentCR) 
			   from @j WHERE (TransactionDate between @MTDFromdate and @EndDate) and 
			  (SubCategoryName='Misc Expense' or SubCategoryName='Misc Income'))
			   WHERE Items='Misc Income/Expense'

UPDATE @EquityItems 
SET Monthly = (SELECT sum(Dividend) 
			 from T_MW_genericPnL WHERE rundate  between @MTDFromdate and @EndDate and Open_CloseTag = 'D'
             and Fund in  (SELECT * from dbo.Split(@Fund, ',')))
		     WHERE Items = 'Gross Dividend Income/Expense'

-------------------------------------------------Quarterly----------------------------------------------------


UPDATE @EquityItems 
SET Quarterly = (SELECT sum( CurrentDR+CurrentCR) from @j 
                 WHERE TransactionDate between @QTDFromdate and @EndDate
                 and SubCategoryName=Items)

UPDATE @EquityItems 
SET Quarterly = isnull((SELECT sum(TotalRealizedPNLOnCost)+Sum(ChangeInUnrealizedPNL) from T_MW_genericPnL
                WHERE (rundate between @QTDFromdate and @EndDate) and asset &lt;&gt; 'cash'
                and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
                WHERE Items='Realized Unrealized Gain/(Loss) on Securities'

UPDATE @EquityItems 
SET Quarterly = isnull((SELECT sum(UnrealizedTotalGainOnCostD2_Base) from T_MW_genericPnL
                WHERE (rundate between @QTDFromdate and @EndDate) and asset = 'cash'
                and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
                WHERE Items='Unrealized Gain On Cash'


UPDATE @EquityItems 
SET Quarterly = (SELECT sum(CurrentDR + CurrentCR) 
			     from @j WHERE (TransactionDate between @QTDFromdate and @EndDate) and 
				(SubCategoryName='Interest Expense' or SubCategoryName='Interest Income'))
			     WHERE Items='Interest Income/Expense'

UPDATE @EquityItems 
SET Quarterly = (SELECT sum(CurrentDR + CurrentCR) 
				 from @j WHERE (TransactionDate between @QTDFromdate and @EndDate) and 
				(SubCategoryName='Misc Expense' or SubCategoryName='Misc Income'))
			     WHERE Items='Misc Income/Expense'


UPDATE @EquityItems 
SET Quarterly = (SELECT sum(Dividend) 
			 from T_MW_genericPnL WHERE (rundate between @QTDFromdate and @EndDate) and Open_CloseTag = 'D'
             and Fund in (SELECT * from dbo.Split(@Fund, ',')))
		     WHERE Items = 'Gross Dividend Income/Expense'

------------------------------------------------Yearly----------------------------------------------------------
UPDATE @EquityItems 
SET Yearly = (SELECT sum( CurrentDR+CurrentCR) from @j 
             WHERE TransactionDate &gt; @YTDFromdate and TransactionDate &lt;=@EndDate
             and SubCategoryName=Items)


UPDATE @EquityItems 
SET Yearly = isnull((SELECT sum(TotalRealizedPNLOnCost)+Sum(ChangeInUnrealizedPNL) from T_MW_genericPnL
             WHERE rundate &gt; @YTDFromdate and rundate  &lt;=@EndDate  and asset &lt;&gt; 'cash'
             and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
             WHERE Items='Realized Unrealized Gain/(Loss) on Securities'

UPDATE @EquityItems 
SET Yearly= 
isnull((SELECT sum(UnrealizedTotalGainOnCostD2_Base) from T_MW_genericPnL
                  WHERE rundate &gt; @YTDFromdate and rundate  &lt;=@EndDate and asset = 'cash'
                  and Fund in  (SELECT * from dbo.Split(@Fund, ',')) ),0)
                   WHERE Items='Unrealized Gain On Cash'

UPDATE @EquityItems 
SET Yearly = (SELECT sum(CurrentDR + CurrentCR) 
			  from @j WHERE (TransactionDate &gt; @YTDFromdate and TransactionDate &lt;=@EndDate) and 
			  (SubCategoryName='Interest Expense' or SubCategoryName='Interest Income'))
			  WHERE Items='Interest Income/Expense'

UPDATE @EquityItems 
SET Yearly = (SELECT sum(CurrentDR + CurrentCR) 
		      from @j WHERE (TransactionDate &gt; @YTDFromdate and TransactionDate &lt;=@EndDate) and 
			 (SubCategoryName='Misc Expense' or SubCategoryName='Misc Income'))
		      WHERE Items='Misc Income/Expense'

UPDATE @EquityItems 
SET Yearly = (SELECT sum(Dividend) 
			 from T_MW_genericPnL WHERE rundate &gt; @YTDFromdate and rundate  &lt;=@EndDate and Open_CloseTag = 'D'
             and Fund in (SELECT * from dbo.Split(@Fund, ',')))
		     WHERE Items = 'Gross Dividend Income/Expense'
-----------------------------------------------------------------------------------------------------

DELETE FROM @EquityItems WHERE items in ('Interest Income','Interest Expense','Income','Expenses','Misc Income','Misc Expense','Cash Contributions')
-----------------------------------------------------------------------------------------------------

--Result SET
SELECT 
items, 
isnull(Daily,0) as 'Daily', 
isnull(monthly,0)as 'Monthly', 
isnull(Quarterly,0)as'Quarterly', 
isnull(yearly,0) as 'Yearly' 
from @EquityItems 
order by Items</CommandText>
