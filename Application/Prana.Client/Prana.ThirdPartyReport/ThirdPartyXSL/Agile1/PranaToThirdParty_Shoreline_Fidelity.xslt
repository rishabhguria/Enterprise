<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" exclude-result-prefixes="xs">
	<xsl:output method="xml" encoding="UTF-8" indent="yes"/>
	<xsl:template match="/ThirdPartyFlatFileDetailCollection">
		<ThirdPartyFlatFileDetailCollection>
			<xsl:attribute name="xsi:noNamespaceSchemaLocation">C:/UpdatedTP/Prana_Final.xsd</xsl:attribute>
			<xsl:for-each select="ThirdPartyFlatFileDetail">
				<ThirdPartyFlatFileDetail>

					<!--<GroupAllocationReq>
						<xsl:value-of select ="'true'"/>
					</GroupAllocationReq>-->
					<FileHeader>
						<xsl:value-of select ="'true'"/>
					</FileHeader>
					<FileFooter>
						<xsl:value-of select ="'true'"/>
					</FileFooter>
					<RowHeader>
						<xsl:value-of select ="'false'"/>
					</RowHeader>

					<TaxlotState>
						<xsl:value-of select ="TaxLotState"/>
					</TaxlotState>

					<TransactionID>
						<xsl:value-of select="''"/>
					</TransactionID>

					<!-- Side Starts-->

					<xsl:choose>
						<xsl:when test="Side='Buy'">
							<TransactionType>
								<xsl:value-of select="'B'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Buy to Open' ">
							<TransactionType>
								<xsl:value-of select="'BTO'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Buy to Cover'">
							<TransactionType>
								<xsl:value-of select="'BC'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Buy to Close' and Asset='Equity'">
							<TransactionType>
								<xsl:value-of select="'BC'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Buy to Close' and Asset='EquityOption'">
							<TransactionType>
								<xsl:value-of select="'BTC'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Sell'">
							<TransactionType>
								<xsl:value-of select="'S'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Sell to Open'">
							<TransactionType>
								<xsl:value-of select="'STO'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Sell to Close'">
							<TransactionType>
								<xsl:value-of select="'STC'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Sell short'">
							<TransactionType>
								<xsl:value-of select="'SS'"/>
							</TransactionType>
						</xsl:when>
						<xsl:otherwise >
							<TransactionType>
								<xsl:value-of select="''"/>
							</TransactionType>
						</xsl:otherwise>
					</xsl:choose >					

				<xsl:choose>
					<xsl:when test ="Asset='EquityOption'">
						<Symbol>
							<xsl:value-of select="translate(IDCOOptionSymbol,' ','')"/>
						</Symbol>
					</xsl:when>
					<xsl:otherwise>
						<Symbol>
							<xsl:value-of select="Symbol"/>
						</Symbol>
					</xsl:otherwise>
				</xsl:choose >

					<xsl:variable name ="varInternationalSymbolCheck">
						<xsl:value-of select="substring-before(Symbol,'-')"/>
					</xsl:variable>

					<xsl:choose>
						<xsl:when test ="Asset='Equity' and $varInternationalSymbolCheck != ''">
							<SEDOL>
								<xsl:value-of select="SEDOL"/>
							</SEDOL>
						</xsl:when>
						<xsl:otherwise>
							<SEDOL>
								<xsl:value-of select="''"/>
							</SEDOL>
						</xsl:otherwise>
					</xsl:choose >

					

					<CUSIP>
						<xsl:value-of select="''"/>
					</CUSIP>

					<Quantity>
						<xsl:value-of select ="AllocatedQty"/>
					</Quantity>

					<xsl:choose>
						<xsl:when test ="$varInternationalSymbolCheck != ''">
							<ExecutingBroker>
								<xsl:value-of select="'FORX'"/>
							</ExecutingBroker>
						</xsl:when>
						<xsl:when test ="CounterParty='FIDE'">
							<ExecutingBroker>
								<xsl:value-of select="'NFSC'"/>
							</ExecutingBroker>
						</xsl:when>
						<xsl:otherwise>
							<ExecutingBroker>
								<xsl:value-of select="CounterParty"/>
							</ExecutingBroker>
						</xsl:otherwise>
					</xsl:choose >

					<Currency>
						<xsl:value-of select="CurrencySymbol"/>
					</Currency>

					<Currency2>
						<xsl:value-of select="CurrencySymbol"/>
					</Currency2>

					<Price>
						<xsl:value-of select="AveragePrice"/>
					</Price>

					<AllocationAccount>
						<xsl:value-of select="AccountNo"/>
					</AllocationAccount>

					<xsl:variable name = "varTradeMth" >
						<xsl:value-of select="substring(TradeDate,1,2)"/>
					</xsl:variable>
					<xsl:variable name = "varTradeDay" >
						<xsl:value-of select="substring(TradeDate,4,2)"/>
					</xsl:variable>
					<xsl:variable name = "varTradeYR" >
						<xsl:value-of select="substring(TradeDate,7,4)"/>
					</xsl:variable>
					<TradeDate>
						<xsl:value-of select="concat($varTradeYR,'',$varTradeMth,'',$varTradeDay)"/>
					</TradeDate>

					<xsl:variable name = "varSettleMth" >
						<xsl:value-of select="substring(SettlementDate,1,2)"/>
					</xsl:variable>
					<xsl:variable name = "varSettleDay" >
						<xsl:value-of select="substring(SettlementDate,4,2)"/>
					</xsl:variable>
					<xsl:variable name = "varSettleYR" >
						<xsl:value-of select="substring(SettlementDate,7,4)"/>
					</xsl:variable>
					<SettleDate>
						<xsl:value-of select="concat($varSettleYR,'',$varSettleMth,'',$varSettleDay)"/>
					</SettleDate>

					<!-- Starts Asset Type -->
					<xsl:choose>
						<xsl:when test="Asset='Equity'">
							<Asset_type>
								<xsl:value-of select="'Stock'"/>
							</Asset_type>
						</xsl:when>
						<xsl:when test="Asset='EquityOption'">
							<Asset_type>
								<xsl:value-of select="'Option'"/>
							</Asset_type>
						</xsl:when>
						<xsl:when test="Asset='FX'">
							<Asset_type>
								<xsl:value-of select="'FX'"/>
							</Asset_type>
						</xsl:when>
						<xsl:otherwise >
							<Asset_type>
								<xsl:value-of select="''"/>
							</Asset_type>
						</xsl:otherwise>
					</xsl:choose>

					<AccruedInterest>
						<xsl:value-of select="0"/>
					</AccruedInterest>

					<!--<Comment1>
						<xsl:value-of select ="''"/>
					</Comment1>

					<Comment2>
						<xsl:value-of select ="''"/>
					</Comment2>-->

					<CommissionType>
						<xsl:value-of select="'G'"/>
					</CommissionType>
					<Commission>
						<xsl:value-of select ="CommissionCharged + TaxOnCommissions + OtherBrokerFee + StampDuty + TransactionLevy + ClearingFee + MiscFees"/>
					</Commission>

					<NetAmount>
						<xsl:value-of select="NetAmount"/>
					</NetAmount>

					<xsl:choose>
						<xsl:when test ="Asset='EquityOption'">
							<OptionSymbol>
								<xsl:value-of select ="UnderlyingSymbol"/>
							</OptionSymbol>
						</xsl:when>
						<xsl:otherwise>
							<OptionSymbol>
								<xsl:value-of select ="''"/>
							</OptionSymbol>
						</xsl:otherwise>
					</xsl:choose>

					<xsl:choose>
						<xsl:when test ="Asset='EquityOption'">
							<OptionExpirationDate>
								<xsl:value-of select ="ExpirationDate"/>
							</OptionExpirationDate>
						</xsl:when>
						<xsl:otherwise>
							<OptionExpirationDate>
								<xsl:value-of select ="''"/>
							</OptionExpirationDate>
						</xsl:otherwise>
					</xsl:choose>

					<xsl:choose>
						<xsl:when test ="Asset='EquityOption'">
							<OptionType>
								<xsl:value-of select ="PutCall"/>
							</OptionType>
						</xsl:when>
						<xsl:otherwise>
							<OptionType>
								<xsl:value-of select ="Call_Put"/>
							</OptionType>
						</xsl:otherwise>
					</xsl:choose>

					<xsl:choose>
						<xsl:when test ="Asset='EquityOption'">
							<OptionStrikePrice>
								<xsl:value-of select ="StrikePrice"/>
							</OptionStrikePrice>
						</xsl:when>
						<xsl:otherwise>
							<OptionStrikePrice>
								<xsl:value-of select ="''"/>
							</OptionStrikePrice>
						</xsl:otherwise>
					</xsl:choose>
					
					
					<ALLOCQTY>
						<xsl:value-of select ="AllocatedQty"/>
					</ALLOCQTY>
					
					<EntityID>
						<xsl:value-of select="EntityID"/>
					</EntityID>

				</ThirdPartyFlatFileDetail>
			</xsl:for-each>
		</ThirdPartyFlatFileDetailCollection>
	</xsl:template>
</xsl:stylesheet>
