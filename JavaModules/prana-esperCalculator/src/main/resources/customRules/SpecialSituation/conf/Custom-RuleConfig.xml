<?xml version="1.0" encoding="ISO-8859-1"?>
<ruleConfig>
     <rules name="SpecialSituation">
         <rule id="custom-rule-SpecialSituations1">
               <name>Account-Issuer Exposure</name>
               <enabled>false</enabled>
               <isDeleted>false</isDeleted>
               <ruleType>PreTrade</ruleType>
               <eplPath>/accountIssuerExposure/</eplPath>
               <description>Restriction on having no more than X%(Y%) of total assets in any issuer for the given Account (Other Accounts)</description>
               <compressionLevel>Account-Issuer</compressionLevel>
               <validationCompletedEventName>PreTradeAccountIssuerExposureCheck</validationCompletedEventName>
               <outputStatementList>
                    <outputStatementName name="PreTradeAccountIssuerExposureCheck"/>
               </outputStatementList>
               <windowFillerStatementList>
                    <windowFillerStatementName name="PreTradeAccountIssuerExposureCheck"/>
               </windowFillerStatementList>
               <constantList>
					<constantValue displayName="Account(s) name" name="Account_Name" type="String" value="PE MS"/>
					<constantValue displayName="Threshold for given account(s)" name="Exposure_Percentage" type="double" value="10"/>
					<constantValue displayName="Threshold for other account(s)" name="Exposure_Percentage_Others" type="double" value="15"/>
					<constantValue displayName="Variable (Net Market Value or Net Exposure)" name="Variable1" type="String" value="Net Market Value"/>
				</constantList>
               <blocked>true</blocked>
          </rule>
