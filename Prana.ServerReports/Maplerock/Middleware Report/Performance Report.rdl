    <DataSet Name="SymbolExposures">
      <Fields>
        <Field Name="Symbol">
          <DataField>Symbol</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SecurityName">
          <DataField>SecurityName</DataField>
