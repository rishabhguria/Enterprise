            <Value>=Parameters!ITD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@fund">
            <Value>=Join(Parameters!fund.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
