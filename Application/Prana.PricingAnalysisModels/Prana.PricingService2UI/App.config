      <add template="{message}&#xD;&#xA;		" type="Microsoft.Practices.EnterpriseLibrary.Logging.Formatters.TextFormatter, Microsoft.Practices.EnterpriseLibrary.Logging" name="Risk Formatter" />
      <add template="{timestamp} : {title}{message}" type="Microsoft.Practices.EnterpriseLibrary.Logging.Formatters.TextFormatter, Microsoft.Practices.EnterpriseLibrary.Logging" name="Basic Messages" />
    </formatters>
    <logFilters>
      <add categoryFilterMode="AllowAllExceptDenied" type="Microsoft.Practices.EnterpriseLibrary.Logging.Filters.CategoryFilter, Microsoft.Practices.EnterpriseLibrary.Logging" name="Category" />
      <add minimumPriority="0" maximumPriority="2147483647" type="Microsoft.Practices.EnterpriseLibrary.Logging.Filters.PriorityFilter, Microsoft.Practices.EnterpriseLibrary.Logging" name="Priority" />
    </logFilters>
    <categorySources>
      <add switchValue="All" name="Default Category">
        <listeners>
          <add name="RollingFile ExceptionListener" />
        </listeners>
      </add>
      <add switchValue="All" name="Category_Tracing">
        <listeners>
          <add name="RollingFile TraceListener" />
        </listeners>
      </add>
      <add switchValue="All" name="Risk_Logging">
        <listeners>
          <add name="RollingFlatFileTraceListener" />
        </listeners>
      </add>
      <add switchValue="All" name="LiveData_Logging">
        <listeners>
          <add name="RollingFlatFileLiveDataListener" />
        </listeners>
      </add>
      <add switchValue="All" name="Category_Information">
        <listeners>
          <add name="RollingInformationsTraceListener" />
        </listeners>
      </add>
      <add switchValue="All" name="Category_Warning">
        <listeners>
          <add name="RollingWarningsTraceListener" />
        </listeners>
      </add>
    </categorySources>
    <specialSources>
      <allEvents switchValue="All" name="All Events" />
      <notProcessed switchValue="All" name="Unprocessed Category" />
      <errors switchValue="All" name="Logging Errors &amp; Warnings">
        <listeners>
          <add name="RollingFile ExceptionListener" />
        </listeners>
      </errors>
    </specialSources>
  </loggingConfiguration>
  <exceptionHandling>
