<?xml version="1.0" encoding="ISO-8859-1" ?>
<ruleConfig>
	<!-- 1. id is primary key. 2. name and rule-Type should be unique in combination -->
	<rules name="TigerVeda">
		<rule id="custom-rule-TigerVeda1">
			<name>Concentration (NAV)</name>
			<enabled>false</enabled>
			<isDeleted>false</isDeleted>
			<ruleType>PreTrade</ruleType>
			<eplPath>/NAVConcentration/</eplPath>
			<description>The sum of all securities that make up at least 5 % of the portfolio make up 40% or more of the portfolio.</description>
			<compressionLevel>MasterFund</compressionLevel>
			<validationCompletedEventName>NAVConcentrationTigerVeda</validationCompletedEventName>
			<outputStatementList>
				<outputStatementName name="NAVConcentrationTigerVeda" />
			</outputStatementList>
			<windowFillerStatementList>
				<windowFillerStatementName name="NAVConcentrationTigerVeda" />
			</windowFillerStatementList>
			<blocked>true</blocked>
			<constantList>
				<constantValue name="Fund_Name" type="String" value="UCITS" displayName="Master Funds to be monitored" />
				<constantValue name="Securities_Percentage" type="double" value="5" displayName="Individual Securities Concentration Limit" />
				<constantValue name="Total_Percentage" type="double" value="40" displayName="Total concentration limit" />			
			</constantList>
		</rule>
		<rule id="custom-rule-TigerVeda2">
			<name>Liquid Limit</name>
			<enabled>false</enabled>
			<isDeleted>false</isDeleted>
			<ruleType>PreTrade</ruleType>
			<eplPath>/LiquidLimit/</eplPath>
			<description>Transaction results in investment in Liquid Securities to fall below the 90% threshold</description>
			<compressionLevel>MasterFund</compressionLevel>
			<validationCompletedEventName>LiquidLimitTigerVeda</validationCompletedEventName>
			<outputStatementList>
				<outputStatementName name="LiquidLimitTigerVeda" />
			</outputStatementList>
			<windowFillerStatementList>
				<windowFillerStatementName name="LiquidLimitTigerVeda" />
			</windowFillerStatementList>
			<blocked>true</blocked>
			<constantList>
				<constantValue name="Fund_Name1" type="String" value="UCITS" displayName="Master Funds to be monitored" />
		   		<constantValue name="Liquid_Securities_Percentage" type="double" value="90" displayName="Liquid Securities Percentage" />
			</constantList>
		</rule>
