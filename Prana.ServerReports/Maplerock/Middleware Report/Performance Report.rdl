        <Field Name="Exposure">
          <DataField>Exposure</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>declare @GrossExposure float
select @GrossExposure=nullif( sum(abs(deltaExposurebase)),0) from t_mw_genericpnl where fund in (select * from dbo.split(@fund,',') )
and open_closetag='O' and rundate=@EndDate


declare @t table
(
 Symbol varchar(max),
 SecurityName varchar(max),
 Exposure float
)

insert into @t
select Symbol, securityName, sum(deltaExposurebase)/@GrossExposure  from t_mw_genericpnl where fund in (select * from dbo.split(@fund,',') )
and open_closetag='O' and rundate=@EndDate and Asset&lt;&gt;'Cash'
group by Symbol, SecurityName

select * from @t</CommandText>
        <QueryParameters>
          <QueryParameter Name="@fund">
            <Value>=Join(Parameters!fund.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="SymbolPnL">
      <Fields>
        <Field Name="Symbol">
          <DataField>Symbol</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SecurityName">
          <DataField>SecurityName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Side">
          <DataField>Side</DataField>
