<?xml version="1.0" encoding="UTF-8"?><ruleConfig>
	<!--  	 	1. id is primary key.
			    2. name and rule-Type should be unique in combination-->
	<rules name="KettleHill">	
		 
		 <rule id="custom-rule-KH1">
			<name>Max Net Industry as per GICS</name>
			<enabled>false</enabled>
			<isDeleted>false</isDeleted>
			<ruleType>PreTrade</ruleType>
			<eplPath>/MaxNetIndustryasPerGICS/</eplPath>
			<description>Contains current Net Exposure for MasterFund Custom UDA1 compression greater than 20%</description>
			<compressionLevel>MasterFund-CustomUDA1</compressionLevel>
			<validationCompletedEventName>MFCustomUDA1ConcentrationKH</validationCompletedEventName>
			<outputStatementList>		
				<outputStatementName name="MFCustomUDA1ConcentrationKH"/>
			</outputStatementList>
			<windowFillerStatementList>		
				<windowFillerStatementName name="MFCustomUDA1ConcentrationKH"/>
			</windowFillerStatementList>
			<blocked>true</blocked>		
			<constantList>
				<constantValue displayName="Master Funds to be monitored" name="MasterFund_Name_KH1" type="String" value="KH1, KH2, LoCorr"/>
		   		<constantValue displayName="% Net Exposure" name="Percentage_NetExposure_KH1" type="double" value="20"/>
			</constantList>			
		</rule> 
			
		  <rule id="custom-rule-KH2">
			<name>Max Gross Sector as per GICS</name>
			<enabled>false</enabled>
			<isDeleted>false</isDeleted>
			<ruleType>PreTrade</ruleType>
			<eplPath>/MaxGrossSectorasPerGICS/</eplPath>
			<description>Contains current Gross Exposure for MasterFund Sector compression greater than 40%</description>
			<compressionLevel>MasterFund-Sector</compressionLevel>
			<validationCompletedEventName>MFSectorConcentrationKH</validationCompletedEventName>
			<outputStatementList>		
				<outputStatementName name="MFSectorConcentrationKH"/>
			</outputStatementList>
			<windowFillerStatementList>		
				<windowFillerStatementName name="MFSectorConcentrationKH"/>
			</windowFillerStatementList>
			<blocked>true</blocked>		
			<constantList>
				<constantValue displayName="Master Funds to be monitored" name="MasterFund_Name_KH2" type="String" value="SEI"/>
		   		<constantValue displayName="% Gross Exposure" name="Percentage_GrossExposure_KH2" type="double" value="40"/>
			</constantList>			
		</rule> 
		
