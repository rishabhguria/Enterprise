        <CommandText>P_MW_GetFXExposure</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Funds">
            <Value>=JOIN(Parameters!Funds.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FutureMVType">
