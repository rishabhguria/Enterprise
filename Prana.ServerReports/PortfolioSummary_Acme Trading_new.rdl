    <DataSet Name="PortfolioComposition">
      <Fields>
        <Field Name="Statistic">
          <DataField>Statistic</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalMktValue">
          <DataField>TotalMktValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="TotalPerEquity">
          <DataField>TotalPerEquity</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>declare @t table
(
 Statistic varchar(max),
 TotalMktValue float,
 TotalPerEquity float
)

insert into @t (Statistic)values ('Accrued Dividend')
insert into @t (Statistic)values ('Cash Balances')
insert into @t (Statistic)values ('Long Market Value')
insert into @t (Statistic)values ('Short Market Value')


update @t set TotalMktValue=
(select sum(endingmarketvaluebase) from t_mw_genericpnl where rundate=@EndDate and fund in (select * from dbo.Split(@Fund, ',')) 
and open_closetag='O'  and Asset &lt;&gt; 'Cash' and side='Long')
where Statistic='Long Market Value'

update @t set TotalMktValue=
(select sum(endingmarketvaluebase) from t_mw_genericpnl where rundate=@EndDate and fund in (select * from dbo.Split(@Fund, ',')) 
and open_closetag='O'  and Asset &lt;&gt; 'Cash' and side='Short')
where Statistic='Short Market Value'

update @t set TotalMktValue=
(select sum(endingmarketvaluebase) from t_mw_genericpnl where rundate=@EndDate and fund in (select * from dbo.Split(@Fund, ',')) 
and open_closetag='O' and Asset='Cash')
where Statistic='Cash Balances'


declare @TotalMktValue float
select @TotalMktValue = sum(TotalMktValue) from @t where Statistic in ('Accrued Dividend','Cash Balances','Long Market Value','Short Market Value')
select @TotalMktValue = NULLIF(@TotalMktValue,'0')
update @t set TotalPerEquity= Round(100*TotalMktValue/@TotalMktValue,2)

select Statistic, isnull(TotalMktValue,0) as 'TotalMktValue',isnull(TotalPerEquity,0) as 'TotalPerEquity' from @t</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Enddate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Fund">
            <Value>=Join(Parameters!fund.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Exposure">
      <Fields>
        <Field Name="Statistic">
          <DataField>Statistic</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalMktValue">
          <DataField>TotalMktValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>Declare @t table
(
 Statistic  varchar(max),
 TotalMktValue float
)

insert into @t(Statistic) values('Gross Exposure')
insert into @t(Statistic) values('Net Exposure')


update @t set TotalMktValue=
ISNULL((select sum(abs(DeltaExposureBase)) from dbo.T_MW_GenericPNL 
WHERE Rundate = @EndDate and open_closetag = 'O'  and Asset &lt;&gt; 'Cash' AND
Fund in (select * from dbo.Split(@Fund, ',')) ),0)
where Statistic='Gross Exposure'

update @t set TotalMktValue=
ISNULL((select sum(abs(DeltaExposureBase)) from dbo.T_MW_GenericPNL 
WHERE Rundate = @EndDate and open_closetag = 'O'  and Asset &lt;&gt; 'Cash' AND
Fund in (select * from dbo.Split(@Fund, ',')) ),0)
where Statistic='Net Exposure'


select * from @t</CommandText>
