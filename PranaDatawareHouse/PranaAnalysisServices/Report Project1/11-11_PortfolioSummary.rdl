set [InputDate] as
(
strtomember(@ToDimTimeDate, CONSTRAINED)
)

set [EndDate] as
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).Lag(10):
      strtomember(@ToDimTimeDate, CONSTRAINED)
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)


set [PreviousDay] as
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).PrevMember.Lag(10):
strtomember(@ToDimTimeDate, CONSTRAINED).PrevMember
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)


set [YearStartDate] as
(
Tail(
crossjoin
(
   {
    strtomember(@ToDimTimeDate, CONSTRAINED).Parent.Parent.Parent.FirstChild.FirstChild.FirstChild.PrevMember.Lag(10)
   :strtomember(@ToDimTimeDate, CONSTRAINED).Parent.Parent.Parent.FirstChild.FirstChild.FirstChild.PrevMember
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)

set [MPYearStartDate] as
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate, CONSTRAINED).Parent.Parent.Parent.FirstChild.FirstChild.FirstChild.Lag(10):
strtomember(@ToDimTimeDate, CONSTRAINED).Parent.Parent.Parent.FirstChild.FirstChild.FirstChild
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)
set [ActualYearStartDate] as
(

    strtomember(@ToDimTimeDate, CONSTRAINED)
    .Parent.Parent.Parent.FirstChild.FirstChild.FirstChild

)

member MarkPriceEndDate
as
(
(EndDate.Item(0),[Measures].[Final Mark Price])
)
member MarkPricePrevDate
as
(
(PreviousDay.Item(0),[Measures].[Final Mark Price])
)

member MarkPriceYearStartDate
as
(
(MPYearStartDate.Item(0),[Measures].[Final Mark Price])
)

member DailyUnRealisedPnl as
(
sum(

  descendants(
               [PM Taxlots].[Symbol-TaxLotID],
               [PM Taxlots].[Symbol-TaxLotID].[Tax Lot ID]
                  )
,
 iif([Measures].[TaxlotTradeDate]&gt;=InputDate.Item(0).Properties("Key",TYPED),
([Measures].[TaxLotOpenQty]
*[Measures].[SideMultiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
*(MarkPriceEndDate-[Measures].[AvgPrice]))
-[Measures].[Open Total Commissionand Fees]
,
[Measures].[TaxLotOpenQty]
*[Measures].[SideMultiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
* (MarkPriceEndDate-MarkPricePrevDate)
)
)
)
member DailyRealisedPnl as
(
sum(
Filter(
crossjoin(inputDate.Item(0):inputDate.Item(0) ,
      [PM Tax Lot Closing Temp].[PM Tax Lot Closing Temp].children),
      not isempty([Measures].[ClosedQty]))
,
 iif([Measures].[Opening Trade Date Key]&gt;=inputDate.Item(0).Properties("Key",TYPED),
([Measures].[ClosedQty]
*[Measures].[Opening Side Multiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
*([Measures].[Closing Price]-[Measures].[Opening Price]))
-[Measures].[Closing Commission]
-[Measures].[Opening Commission]
,
[Measures].[ClosedQty]
*[Measures].[Opening Side Multiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
* ([Measures].[Closing Price]-MarkPricePrevDate)
-[Measures].[Closing Commission]
)
)
)

member [Daily] as 
(
[DailyRealisedPnl]+[DailyUnRealisedPnl]
)

MEMBER [LongDaily] AS
(
([Daily],[Dim DW Account].[Account Type].&amp;[1])
)
MEMBER [ShortDaily] AS
(
([Daily],[Dim DW Account].[Account Type].&amp;[2])
)




