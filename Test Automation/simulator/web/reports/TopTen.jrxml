	<queryString><![CDATA[SELECT inst.Symbol AS Symbol, SUM(IFNULL(ordReq.OrderQty, 0)) AS n
FROM OrderSingle ord, 
          OrderRequest ordReq,
          Instrument inst
WHERE ord.InstrumentId = inst.InstrumentId
AND ord.OrderChainID = ordReq.OrderChainID
AND ordReq.RequestNo = 0
GROUP BY inst.Symbol
ORDER BY  n DESC, Symbol
LIMIT 10]]></queryString>
