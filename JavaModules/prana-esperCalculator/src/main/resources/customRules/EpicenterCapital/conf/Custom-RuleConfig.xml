			<validationCompletedEventName>PreTradeNonBenchMarkCountries</validationCompletedEventName>
			<outputStatementList>
				<outputStatementName name="PreTradeNonBenchMarkCountries"/>
			</outputStatementList>
			<windowFillerStatementList>
				<windowFillerStatementName name="PreTradeNonBenchMarkCountries"/>
			</windowFillerStatementList>
			<blocked>true</blocked>
