    <section name="Compliance" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <log4net>
    <appender name="RollingFileAppenderCommon" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <file value="Logs_new\" />
      <datePattern value="'Error_'MM_dd_yyyy'.log'" />
      <immediateFlush value="true" />
      <param name="appendToFile" value="true" />
      <param name="rollingStyle" value="composite" />
      <param name="maxSizeRollBackups" value="100" />
      <param name="maximumFileSize" value="1MB" />
      <param name="staticLogFileName" value="false" />
      <param name="threshold" value="Error" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]\r\n" />
        <param name="Footer" value="[Footer]\r\n" />
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
      </layout>
    </appender>
    <appender name="RollingFileAppenderClosing" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <file value="Logs_new\Closing\" />
      <datePattern value="'Closing_'MM_dd_yyyy'.log'" />
      <immediateFlush value="true" />
      <param name="appendToFile" value="true" />
      <param name="rollingStyle" value="composite" />
      <param name="maxSizeRollBackups" value="100" />
      <param name="maximumFileSize" value="1MB" />
      <param name="staticLogFileName" value="false" />
      <param name="threshold" value="DEBUG" />
      <!--INFO, DEBUG, WARN, ERROR-->
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]\r\n" />
        <param name="Footer" value="[Footer]\r\n" />
        <param name="ConversionPattern" value="%d %-5p %m%n" />
      </layout>
      <filter type="log4net.Filter.StringMatchFilter">
        <stringToMatch value="ClosingService" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="RollingFileAppenderClosingError" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <file value="Logs_new\Closing\" />
      <datePattern value="'ClosingError_'MM_dd_yyyy'.log'" />
      <immediateFlush value="true" />
      <param name="appendToFile" value="true" />
      <param name="rollingStyle" value="composite" />
      <param name="maxSizeRollBackups" value="100" />
      <param name="maximumFileSize" value="1MB" />
      <param name="staticLogFileName" value="false" />
      <param name="threshold" value="ERROR" />
      <!--INFO, DEBUG, WARN, ERROR-->
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]\r\n" />
        <param name="Footer" value="[Footer]\r\n" />
        <param name="ConversionPattern" value="%d %-5p %m%n" />
      </layout>
      <filter type="log4net.Filter.StringMatchFilter">
        <stringToMatch value="ClosingService" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="RollingFileAppenderAllocation" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <file value="Logs_new\Allocation\" />
      <datePattern value="'Allocation_'MM_dd_yyyy'.log'" />
      <immediateFlush value="true" />
      <param name="appendToFile" value="true" />
      <param name="rollingStyle" value="composite" />
      <param name="maxSizeRollBackups" value="100" />
      <param name="maximumFileSize" value="1MB" />
      <param name="staticLogFileName" value="false" />
      <param name="threshold" value="DEBUG" />
      <!--INFO, DEBUG, WARN, ERROR-->
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]\r\n" />
        <param name="Footer" value="[Footer]\r\n" />
        <param name="ConversionPattern" value="%d %-5p %m%n" />
      </layout>
      <filter type="log4net.Filter.StringMatchFilter">
        <stringToMatch value="AllocationService" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="RollingFileAppenderAllocationError" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <file value="Logs_new\Allocation\" />
      <datePattern value="'AllocationError_'MM_dd_yyyy'.log'" />
      <immediateFlush value="true" />
      <param name="appendToFile" value="true" />
      <param name="rollingStyle" value="composite" />
      <param name="maxSizeRollBackups" value="100" />
      <param name="maximumFileSize" value="1MB" />
      <param name="staticLogFileName" value="false" />
      <param name="threshold" value="ERROR" />
      <!--INFO, DEBUG, WARN, ERROR-->
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]\r\n" />
        <param name="Footer" value="[Footer]\r\n" />
        <param name="ConversionPattern" value="%d %-5p %m%n" />
      </layout>
      <filter type="log4net.Filter.StringMatchFilter">
        <stringToMatch value="AllocationService" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
