            <!--
              FlatFileAdapter acts as both a listener and a source message
              processor. It uses two file system persistent queues; one for
              writing messages from FIX (the "fromFix" queue) and the other for
              messages to be sent to FIX (the "toFix" queue). The other party is
              expected to read from the "fromFix" queue and write to the "toFix"
              queue.

              By default, it is assumed that the queue files contain FIX
              messages and that session status events (onConnection, onReset
              and onException) are not passed through the adapter.
            -->
            <!ELEMENT FlatFileAdapter ANY>
            <!ATTLIST FlatFileAdapter
                      class CDATA #FIXED "com.cameronsystems.fix.flatfileadapter.processor.FlatFileAdapter"
                      id CDATA #IMPLIED
                      peerWriter CDATA "com.cameronsystems.fix.flatfileadapter.processor.DefaultFlatFileWriter"
                      fromFixQueue CDATA #IMPLIED
                      peerReader CDATA "com.cameronsystems.fix.flatfileadapter.processor.DefaultFlatFileReader"
                      peerRecoveryInterval CDATA "10"
                      peerPollingInterval CDATA "10"
                      toFixQueue CDATA #IMPLIED
                      toFixErrorQueue CDATA #IMPLIED
                      transformer CDATA "com.cameronsystems.fix.message.RawTransformer"
            >

