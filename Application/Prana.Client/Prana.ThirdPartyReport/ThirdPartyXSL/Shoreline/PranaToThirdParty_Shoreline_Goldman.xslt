<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" exclude-result-prefixes="xs">
	<xsl:output method="xml" encoding="UTF-8" indent="yes"/>
	<xsl:template match="/ThirdPartyFlatFileDetailCollection">
		<ThirdPartyFlatFileDetailCollection>
			<xsl:attribute name="xsi:noNamespaceSchemaLocation">C:/UpdatedTP/Prana_Final.xsd</xsl:attribute>
			<xsl:for-each select="ThirdPartyFlatFileDetail">
				<ThirdPartyFlatFileDetail>

					<RowHeader>
						<xsl:value-of select ="'false'"/>
					</RowHeader>

					<TaxlotState>
						<xsl:value-of select ="TaxLotState"/>
					</TaxlotState>

					<OrderNumber>
						<xsl:value-of select ="TradeRefID"/>
					</OrderNumber>

					<xsl:choose>
						<xsl:when test="TaxLotState='Allocated'">
							<CancelCorrectIndicator>
								<xsl:value-of select="'N'"/>
							</CancelCorrectIndicator>
						</xsl:when>
						<xsl:when test="TaxLotState='Amemded'">
							<CancelCorrectIndicator>
								<xsl:value-of select="'A'"/>
							</CancelCorrectIndicator>
						</xsl:when>
						<xsl:when test="TaxLotState='Deleted'">
							<CancelCorrectIndicator>
								<xsl:value-of select="'C'"/>
							</CancelCorrectIndicator>
						</xsl:when>
						<xsl:otherwise>
							<CancelCorrectIndicator>
								<xsl:value-of select="'N'"/>
							</CancelCorrectIndicator>
						</xsl:otherwise>
					</xsl:choose >


					<AccountNumber>
						<xsl:value-of select="FundAccountNo"/>
					</AccountNumber>

					<SecurityIdentifier>
						<xsl:value-of select ="Symbol"/>
					</SecurityIdentifier>

					<Broker>
						<xsl:value-of select ="CounterParty"/>
					</Broker>

					<Custodian>
						<xsl:value-of select="'GSCO'"/>
					</Custodian>


					<!-- Side Starts-->

					<xsl:choose>
						<xsl:when test="Side='Buy' or Side='Buy to Open' ">
							<TransactionType>
								<xsl:value-of select="'B'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Buy to Cover' or Side='Buy to Close'">
							<TransactionType>
								<xsl:value-of select="'BC'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Sell' or Side='Sell to Close'">
							<TransactionType>
								<xsl:value-of select="'S'"/>
							</TransactionType>
						</xsl:when>
						<xsl:when test="Side='Sell short' or Side='Sell to Open'">
							<TransactionType>
								<xsl:value-of select="'SS'"/>
							</TransactionType>
						</xsl:when>
						<xsl:otherwise >
							<TransactionType>
								<xsl:value-of select="' '"/>
							</TransactionType>
						</xsl:otherwise>
					</xsl:choose >

					<CurrencyCode>
						<xsl:value-of select="CurrencySymbol"/>
					</CurrencyCode>

					<xsl:variable name = "varTradeMth" >
						<xsl:value-of select="substring(TradeDate,1,2)"/>
					</xsl:variable>
					<xsl:variable name = "varTradeDay" >
						<xsl:value-of select="substring(TradeDate,4,2)"/>
					</xsl:variable>
					<xsl:variable name = "varTradeYR" >
						<xsl:value-of select="substring(TradeDate,9,2)"/>
					</xsl:variable>
					<TradeDate>
						<xsl:value-of select="concat($varTradeMth,'/',$varTradeDay,'/',$varTradeYR)"/>
					</TradeDate>

					<xsl:variable name = "varSettleMth" >
						<xsl:value-of select="substring(SettlementDate,1,2)"/>
					</xsl:variable>
					<xsl:variable name = "varSettleDay" >
						<xsl:value-of select="substring(SettlementDate,4,2)"/>
					</xsl:variable>
					<xsl:variable name = "varSettleYR" >
						<xsl:value-of select="substring(SettlementDate,9,2)"/>
					</xsl:variable>
					<SettleDate>
						<xsl:value-of select="concat($varSettleMth,'/',$varSettleDay,'/',$varSettleYR)"/>
					</SettleDate>

					<Quantity>
						<xsl:value-of select ="AllocatedQty"/>
					</Quantity>

					<Commission>
						<xsl:value-of select ="CommissionCharged + TaxOnCommissions"/>
					</Commission>

					<Price>
						<xsl:value-of select="AveragePrice"/>
					</Price>

					<AccruedInterest>
						<xsl:value-of select ="0"/>
					</AccruedInterest>

					<TradeTax>
						<xsl:value-of select ="0"/>
					</TradeTax>

					<MiscMoney>
						<xsl:value-of select="0"/>
					</MiscMoney>

					<NetAmount>
						<xsl:value-of select="0"/>
					</NetAmount>

					<Principal>
						<xsl:value-of select="0"/>
					</Principal>

					<Description>
						<xsl:value-of select="FullSecurityName"/>
					</Description>

					<SecuirtyFees>
						<xsl:value-of select ="0"/>
					</SecuirtyFees>

					<EntityID>
						<xsl:value-of select="EntityID"/>
					</EntityID>

				</ThirdPartyFlatFileDetail>
			</xsl:for-each>
		</ThirdPartyFlatFileDetailCollection>
	</xsl:template>
</xsl:stylesheet>
