        <CommandText>with 
set [InputDate] as
(
strtomember(@ToDimTimeDate, CONSTRAINED)
)

set [EndDate] as
(
Tail(
crossjoin
(
   {
      strtomember(@ToDimTimeDate).Lag(10):strtomember(@ToDimTimeDate)
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)
set [FirstDate] as
(
Tail(
crossjoin
(
   {
      strtomember(@FirstDimTime).Lag(10):strtomember(@FirstDimTime)
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)

set [PreviousDay] as
(
Tail(
crossjoin
(
   {
      strtomember(@FirstDimTime, CONSTRAINED).PrevMember.Lag(10):
strtomember(@FirstDimTime,CONSTRAINED).PrevMember
   } ,{[Dim Time].[Is Holiday].&amp;[False]}
)

,1).item(0).item(0)
)

member MarkPriceEndDate
as
(
(EndDate.Item(0),[Measures].[Final Mark Price])
)
member MarkPriceStartdate
as
(
(PreviousDay.item(0),[measures].[final mark price])
)


member [SymbolLastQuantity] as
(
Sum(Tail(
Filter(
{(null)
:
(PreviousDay.item(0))}*
[PM Open Position Snap Shots].[Symbol].Currentmember,
not isempty([Measures].[SymbolQuantity])
),1),[Measures].[SymbolQuantity]
)
)
member [BeginningMarketValue] as
(
sum(
[PM Open Position Snap Shots].[Symbol].children,
[SymbolLastQuantity]*([Measures].[Final Mark Price],[Dim Time].[Calender].CurrentMember)
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
)
)

member UnRealisedPnl_FirstToEndDate as
(
sum(

  descendants(
               [PM Taxlots].[Symbol-TaxLotID],
               [PM Taxlots].[Symbol-TaxLotID].[Tax Lot ID]
                  )
,
 iif([Measures].[TaxlotTradeDate]&gt;=FirstDate.Item(0).Properties("Key",TYPED),
([Measures].[TaxLotOpenQty]
*[Measures].[SideMultiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
*(MarkPriceEndDate-[Measures].[AvgPrice]))
-[Measures].[Open Total Commissionand Fees]
,
[Measures].[TaxLotOpenQty]
*[Measures].[SideMultiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
* (MarkPriceEndDate-MarkPriceStartdate)
)
)
)
member RealisedPnl_FirstToEndDate as
(
sum(
Filter(
crossjoin(FirstDate.Item(0):inputDate.Item(0) ,
      [PM Tax Lot Closing Temp].[PM Tax Lot Closing Temp].children),
      not isempty([Measures].[ClosedQty]))
,
 iif([Measures].[Opening Trade Date Key]&gt;=FirstDate.Item(0).Properties("Key",TYPED),
([Measures].[ClosedQty]
*[Measures].[Opening Side Multiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
*([Measures].[Closing Price]-[Measures].[Opening Price]))
-[Measures].[Closing Commission]
-[Measures].[Opening Commission]
,
[Measures].[ClosedQty]
*[Measures].[Opening Side Multiplier]
*[Measures].[CurrencyConversionRate]
*[Measures].[Multiplier]
* ([Measures].[Closing Price]-MarkPriceStartdate)
-[Measures].[Closing Commission]
)
)
)



member LongMtmPnl as
(
([Dim Sub Accounts].[SubAccount].&amp;[2000],UnRealisedPnl_FirstToEndDate)+
([Dim Sub Accounts].[SubAccount].&amp;[2000],RealisedPnl_FirstToEndDate)
)
member ShortMtmPnl as
(
([Dim Sub Accounts].[SubAccount].&amp;[2001],UnRealisedPnl_FirstToEndDate)+
([Dim Sub Accounts].[SubAccount].&amp;[2001],RealisedPnl_FirstToEndDate)
)

member AverageCostBasis as 
(
sum(

(FirstDate.Item(0):EndDate.Item(0))*[Dim Sub Accounts].[Type ID].&amp;[4]
,
(
	(
	   EndDate.item(0).Properties("Key",Typed)-
	   [Dim Time].[Calender].CurrentMember.Properties("Key",Typed)+1
	)
*[Measures].[Cash Value]
)/
   (
	   EndDate.item(0).Properties("Key",Typed)-
	    FirstDate.item(0).Properties("Key",Typed)+1
	)
)
)
member [MonthlyACB] as
(
([AverageCostBasis])+
([BeginningMarketValue],[PreviousDay].item(0))+
([Measures].[Cash Value],PreviousDay.item(0),[Dim Sub Accounts].[AccountName].&amp;[Cash Balances])
)

member [MonthlyACBAll] as 
(
([PM Open Position Snap Shots].[Sector].[ALL],MonthlyACB)
)
member [Longreturn] as
(
IIF(MonthlyACBAll=0,0,
(LongMtmPnl)/ [MonthlyACBAll])
)
member [ShortReturn] as
(
IIF(MonthlyACBAll=0,0,
(ShortMtmPnl)/ [MonthlyACBAll])
)

