        </Field>
        <Field Name="PnL">
          <DataField>PnL</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>declare @MTDFromdate datetime,@TotalPnL float
select @MTDFromdate = CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(@EndDate)-1),@EndDate),101)


select @TotalPnL= sum(TotalRealizedPnLMTM+TotalUnrealizedPnLMTM+Dividend) from T_MW_Genericpnl
Where fund in (select * from dbo.split(@fund,',')) and rundate between @MTDFromdate and @EndDate


declare @t table
(
 Symbol varchar(max),
 SecurityName varchar(max),
 Side varchar(max),
 PnL float
)

insert into @t
select Symbol, SecurityName,Side,sum(TotalRealizedPnLMTM+TotalUnrealizedPnLMTM+Dividend)/@TotalPnL  from T_MW_Genericpnl
Where fund in (select * from dbo.split(@fund,',')) and rundate between @MTDFromdate and @EndDate
group by Symbol, SecurityName, Side

select * from @t</CommandText>
