<!ELEMENT Scheduling ((Task | Schedule | EventList)*)>
<!ATTLIST Scheduling
          active ( true | false ) "true"
>

  <!ELEMENT Task ANY>
  <!--
    Task must have either an id or a refid
  -->
  <!ATTLIST Task
            class              CDATA #IMPLIED
            id                 CDATA #IMPLIED
            refid              CDATA #IMPLIED
  >

  <!ELEMENT Schedule ((Schedule | Include | Exclude)*)>
  <!ATTLIST Schedule
            id                 CDATA #IMPLIED
            refid              CDATA #IMPLIED
            timeZone           CDATA #IMPLIED
  >

    <!ELEMENT Include (Date*)>
    <!ATTLIST Include
              hour             CDATA #IMPLIED
              minute           CDATA #IMPLIED
              dayOfWeek        CDATA #IMPLIED
              dayOfMonth       CDATA #IMPLIED
              month            CDATA #IMPLIED
              year             CDATA #IMPLIED
    >

    <!ELEMENT Exclude (Date*)>
    <!ATTLIST Exclude
              hour             CDATA #IMPLIED
              minute           CDATA #IMPLIED
              dayOfWeek        CDATA #IMPLIED
              dayOfMonth       CDATA #IMPLIED
              month            CDATA #IMPLIED
              year             CDATA #IMPLIED
    >

      <!ELEMENT Date EMPTY>
      <!ATTLIST Date
                hour             CDATA #IMPLIED
                minute           CDATA #IMPLIED
                dayOfWeek        CDATA #IMPLIED
                dayOfMonth       CDATA #IMPLIED
                month            CDATA #IMPLIED
                year             CDATA #IMPLIED
      >

  <!ELEMENT EventList ((EventList | Event)*)>
  <!ATTLIST EventList
            id                 CDATA #IMPLIED
            refid              CDATA #IMPLIED
  >

    <!ELEMENT Event (Task, Schedule) >
