<?xml version="1.0" encoding="ISO-8859-1" ?>
<ruleConfig>
	<!--  	 	1. id is primary key.
			    2. name and rule-Type should be unique in combination-->
	<rules name="Iguana">	
	
		 <rule id="custom-ruleIG1">
			<name>Concentration Limits-5</name>
			<enabled>true</enabled>
			<isDeleted>false</isDeleted>
			<ruleType>PostTrade</ruleType>
			<eplPath>/concentrationLimit5/</eplPath>
			<description>The size of the top 5 long positions in Whitney Capital-JPM exceeded 30% of the client Gross Market Value.
			</description>
			<compressionLevel>Account</compressionLevel>
			<validationCompletedEventName></validationCompletedEventName>
			<outputStatementList>		
				<outputStatementName name="ConcentrationLimits5"/>
			</outputStatementList>
			<windowFillerStatementList>		
				<windowFillerStatementName name="ConcentrationLimits5"/>
			</windowFillerStatementList>
			<blocked>true</blocked>			
		</rule> 
