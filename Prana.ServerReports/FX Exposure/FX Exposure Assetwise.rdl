        <CommandText>declare @t table
(
 Symbolt varchar(max),
 SecurityName varchar(max),
 Asset varchar(max),
 TradeCurrency varchar(max),
 MV float,
 NewMV float,
 VSCurrencyIdt int,
 LeadCurrencyIDt int,
 FXrate float
-- FromCurrencyIDt int
)

declare @T_entity table
(
 FundNames varchar(max),
 FundNameID int
)

insert into @T_entity (FundNames)
 select * from dbo.split(@Funds,',')

update @T_entity set FundNameID=FundNameID from t_companyfunds where FundNames=FundName


insert into @t (Symbolt,SecurityName,asset,tradecurrency,MV)
select symbol, SecurityName, asset, tradecurrency,sum(endingMarketValueLocal) as 'MV'
from t_ts_genericpnl 
where rundate=@StartDate and Fund in( select * from dbo.split(@Funds,',')) and open_closetag='O'and Asset not in ('Future','FX')
group by symbol, asset, tradecurrency,SecurityName

insert into @t (Symbolt,SecurityName, asset,tradecurrency,MV)
select symbol, SecurityName, asset, tradecurrency,sum(-Sidemultiplier*unitcostlocal*beginningquantity) as 'MV'
from t_ts_genericpnl 
where rundate=@StartDate and Fund in( select * from dbo.split(@Funds,',')) and open_closetag='O'and Asset = 'FX'
group by symbol, asset, tradecurrency,SecurityName

insert into @t (symbolt,SecurityName, asset,MV)
select symbol, SecurityName, asset,sum(Sidemultiplier*beginningquantity) as 'MV'
from t_ts_genericpnl 
where rundate=@StartDate and Fund in( select * from dbo.split(@Funds,',')) and open_closetag='O'and Asset = 'FX'
group by symbol, asset, tradecurrency,SecurityName


if @FutureMVType='Unrealized P&amp;L on Cost'
begin
insert into @t (Symbolt,SecurityName, asset,tradecurrency,MV)
select symbol, SecurityName, asset, tradecurrency,sum(UnrealizedtotalGainonCostD2_local)as 'MV'
from t_ts_genericpnl 
where rundate=@StartDate and Fund in( select * from dbo.split(@Funds,',')) and open_closetag='O' and Asset='Future'
group by symbol, asset, tradecurrency,SecurityName
end


if @FutureMVType='Day P&amp;L' 
begin
insert into @t (Symbolt,SecurityName, asset,tradecurrency,MV)
select symbol,SecurityName,  asset, tradecurrency,sum((endingpriceLocal-beginningpriceLocal)*beginningquantity*sidemultiplier*multiplier) as 'MV'
from t_ts_genericpnl 
where rundate=@StartDate and Fund in( select * from dbo.split(@Funds,',')) and open_closetag='O'and Asset='Future' and beginningpricelocal&lt;&gt;'0'
group by symbol, asset, tradecurrency,SecurityName

insert into @t (Symbolt,SecurityName, asset,tradecurrency,MV)
select symbol, SecurityName, asset, tradecurrency,sum((endingpriceLocal-unitcostLocal)*beginningquantity*sidemultiplier*multiplier) as 'MV'
from t_ts_genericpnl 
where rundate=@StartDate and Fund in( select * from dbo.split(@Funds,',')) and open_closetag='O'and Asset='Future' and beginningpricelocal='0'
group by symbol, asset, tradecurrency,SecurityName
end



if @FutureMVType='Market Value' 
begin
insert into @t (Symbolt,SecurityName, asset,tradecurrency,MV)
select symbol, SecurityName, asset, tradecurrency,sum(EndingPricelocal*BeginningQuantity*sidemultiplier*Multiplier) as 'MV'
from t_ts_genericpnl 
where rundate=@StartDate and Fund in( select * from dbo.split(@Funds,',')) and open_closetag='O' and Asset='Future'
group by symbol, asset, tradecurrency,SecurityName
end

---------------------------------------For FX---------------------------------------------------------------------

update @t set 
tradeCurrency =leadcurrency, VSCurrencyIDt=VSCurrencyID, LeadCurrencyIDt=LeadCurrencyID from dbo.V_SecMasterData_WithUnderlying  where tickersymbol=  Symbolt and tradecurrency is null and Asset='FX'


insert into @t (asset,MV, LeadCurrencyIDT)
select  'Cash',CashValueLocal,LocalCurrencyID from PM_CompanyFundCashCurrencyValue where date=@StartDate


update @t set TradeCurrency=CurrencySymbol from T_Currency where CurrencyID=LeadCurrencyIDt


select * from @t</CommandText>
        <QueryParameters>
