    <DataSet Name="FXRate">
      <Fields>
        <Field Name="FX_Rate">
          <DataField>FX Rate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CNY">
          <DataField>CNY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rate">
          <DataField>Rate</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>declare  @FXConversionRates Table                       
(                                                                                                                                                  
 FromCurrencyID int,         
 ToCurrencyID int,                                               
 RateValue float,                 
 ConversionMethod int,                                                                             
 Date DateTime,                        
 eSignalSymbol varchar(max)
                                                 
)  

Insert into @FXConversionRates        Exec P_GetAllFXConversionRatesForGivenDateRange @StartDate,@StartDate                                                                                                                             
                                                        
 Update @FXConversionRates                                                                           
 Set RateValue = 1.0/RateValue                                                                                                                                
 Where RateValue &lt;&gt; 0 and ConversionMethod = 1                                                                                                                                
                                                                                     
Update @FXConversionRates                                                                                                                             
 Set RateValue = 0                                                        
 Where RateValue is Null      

select 'FX Rate' as 'FX Rate',substring(esignalsymbol,1,3)as 'CNY', RateValue as 'Rate' from @FXConversionRates where TocurrencyID='1'</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
